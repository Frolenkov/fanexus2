AAzCA,CAAA","sourcesContent":["<template>\r\n  <div v-if=\"data\" class=\"blog\">\r\n    <img :src=\"data.coverImage.src\" class=\"blog-image\" />\r\n    <div class=\"blog-header\">\r\n      <avatar\r\n        class=\"blog-header__avatar\"\r\n        size=\"xl\"\r\n        :file=\"data.Alias.avatar.src\"\r\n      />\r\n      <tabs :tabs=\"navTabs\" />\r\n    </div>\r\n    <div class=\"blog-content\">\r\n      <div class=\"blog-wrapper\">\r\n        <sidebar-info\r\n          :name=\"data.name\"\r\n          :description=\"data.description\"\r\n          :links=\"data.links\"\r\n        />\r\n      </div>\r\n      <div class=\"blog-main\"></div>\r\n      <div class=\"blog-wrapper\">\r\n        <sidebar-tags :tags=\"data.Tags\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getBlog } from '@/services/blog.service';\r\nimport Avatar from '~common/Avatar';\r\nimport Tabs from '~common/Tabs';\r\nimport tabsMixin from '@/mixins/tabs.mixin';\r\nimport SidebarInfo from '~common/SidebarInfo';\r\nimport SidebarTags from '~common/SidebarTags';\r\n\r\nexport default {\r\n  props: {\r\n    name: {\r\n      required: true\r\n    }\r\n  },\r\n  components: {\r\n    Avatar,\r\n    Tabs,\r\n    SidebarInfo,\r\n    SidebarTags\r\n  },\r\n  mixins: [tabsMixin],\r\n  data() {\r\n    return {\r\n      data: null,\r\n      currentPage: 1,\r\n      navTabs: [\r\n        {\r\n          title: 'Create account'\r\n        },\r\n        {\r\n          title: 'Pass an email'\r\n        },\r\n        {\r\n          title: 'Reset password'\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    async getData() {\r\n      try {\r\n        this.data = await getBlog(this.name, this.currentPage);\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    }\r\n  },\r\n  async mounted() {\r\n    await this.getData();\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.blog {\r\n  grid-area: content;\r\n  overflow: auto;\r\n  background-color: var(--BG-primary);\r\n  &-image {\r\n    height: 300px;\r\n    width: 100%;\r\n    object-fit: cover;\r\n  }\r\n  &-header {\r\n    display: grid;\r\n    align-items: end;\r\n    grid-template-rows: minmax(0, 60px);\r\n    grid-template-columns: 200px max-content 200px;\r\n    grid-gap: var(--element-margin-xl);\r\n    background-color: var(--BG-blog-post);\r\n    padding: 0 var(--element-padding-xl);\r\n    margin-bottom: var(--element-margin-xl);\r\n    &__avatar {\r\n      border: var(--border-width-avatar) solid var(--color-white);\r\n    }\r\n  }\r\n  &-content {\r\n    padding: 0 var(--element-padding-xl);\r\n    display: grid;\r\n    grid-template-columns: 200px auto 200px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}