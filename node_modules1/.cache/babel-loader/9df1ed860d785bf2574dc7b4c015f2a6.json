BACA,KAAA,CAAA,OAAA,EADA;;AAAA;AAEA,cAAA,KAAA,CAAA,MAAA,GAAA,IAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA;AA5CA,CAAA","sourcesContent":["<template>\r\n  <section class=\"likes-blog-posts\">\r\n    <!-- @TODO will be revised in https://temperance-software.atlassian.net/browse/FAN-210 -->\r\n    <blog-all-posts\r\n      :blog-posts=\"data.BlogPosts\"\r\n      v-if=\"loaded\"\r\n      :current-page=\"currentPage\"\r\n      @onPageChange=\"onPageChange\"\r\n    />\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from 'vuex';\r\nimport { feedTypes } from '@/services/feed.service';\r\nimport BlogAllPosts from '@/components/Blog/BlogAllPosts';\r\n\r\nexport default {\r\n  props: {\r\n    name: {\r\n      required: true,\r\n      type: [Number, String]\r\n    }\r\n  },\r\n  components: {\r\n    BlogAllPosts\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      data: state => state.feed.data\r\n    })\r\n  },\r\n  data() {\r\n    return {\r\n      loaded: false,\r\n      currentPage: 1\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions(['getFeedData']),\r\n    getData() {\r\n      return this.getFeedData({\r\n        name: this.name,\r\n        pagination: this.currentPage,\r\n        type: feedTypes.likes\r\n      });\r\n    },\r\n    onPageChange(page) {\r\n      this.currentPage = page;\r\n      this.$router.push({\r\n        name: this.$route.name,\r\n        params: {\r\n          pagination: page\r\n        }\r\n      });\r\n      return this.getData();\r\n    }\r\n  },\r\n  async mounted() {\r\n    await this.getData();\r\n    this.loaded = true;\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.likes-blog-posts {\r\n  padding: var(--element-padding-xl);\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Likes"}]}