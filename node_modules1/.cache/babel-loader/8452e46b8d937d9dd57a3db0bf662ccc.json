AAA,eAAA,uEAAA,KAAA,aAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,OADA,GACA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,aADA;AAEA,kBAAA,cAAA,EAAA;AAFA,iBADA;AAAA;AAAA;AAAA,uBAMA,yBAAA,CAAA,MAAA,CAAA,IAAA,CAAA,EAAA,EAAA,OAAA,CANA;;AAAA;AAOA,gBAAA,MAAA,CAAA,IAAA,CAAA,aAAA,GAAA,KAAA;AAPA;AAAA;;AAAA;AAAA;AAAA;AAAA,kDASA,KATA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAtCA;AAuCA,IAAA,oBAvCA,kCAuCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,YAAA,CAAA,MAAA,CAAA,IAAA,CAAA,EAAA,EAAA,eAAA,CAAA,WAAA,CAAA,CAFA;;AAAA;AAAA;AAAA,uBAGA,MAAA,CAAA,OAAA,EAHA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA,kDAKA,KALA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KA9CA;AA+CA,IAAA,wBA/CA,sCA+CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,YAAA,CAAA,MAAA,CAAA,IAAA,CAAA,EAAA,EAAA,eAAA,CAAA,WAAA,CAAA,CAFA;;AAAA;AAAA;AAAA,uBAGA,MAAA,CAAA,OAAA,EAHA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA,kDAKA,KALA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAtDA;AAuDA,IAAA,aAvDA,2BAuDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,wBADA;AAEA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA,MAAA,CAAA,IAAA,CAAA;AAAA;AAFA,iBAAA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KA5DA;AA6DA,IAAA,eA7DA,6BA6DA;AACA,WAAA,aAAA,CAAA,IAAA,GAAA,IAAA;AACA,KA/DA;AAgEA,IAAA,YAhEA,0BAgEA;AAAA,UAAA,QAAA,uEAAA,CAAA;AAAA,UAAA,cAAA,uEAAA,CAAA;AACA,WAAA,kBAAA,CAAA,QAAA;AACA,WAAA,gBAAA,GAAA,KAAA,mBAAA,CAAA,cAAA,CAAA;AACA;AAnEA,IA/DA;AAoIA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,kBACA,EADA,EACA;AACA,WAAA,aAAA,CAAA,EAAA;AACA;AAHA,GApIA;AAyIA,EAAA,OAzIA,qBAyIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,OAAA,EADA;;AAAA;AAEA,cAAA,MAAA,CAAA,YAAA;;AACA,kBAAA,MAAA,CAAA,MAAA,EAAA;AACA,gBAAA,MAAA,CAAA,aAAA,CAAA,MAAA,CAAA,MAAA;AACA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA;AA/IA,CAAA","sourcesContent":["<template>\r\n  <div v-if=\"data\" class=\"community\">\r\n    <!-- @TODO revise it -->\r\n    <wallpaper :src=\"data.coverImage ? data.coverImage.src : ''\" />\r\n    <div class=\"community-header\">\r\n      <div class=\"community-wrapper\"></div>\r\n      <tabs\r\n        :tabs=\"navTabs\"\r\n        @selectTab=\"$active => setComponent($active, $active)\"\r\n        :active=\"activeTab\"\r\n      />\r\n    </div>\r\n    <div class=\"community-content\">\r\n      <div class=\"community-wrapper\">\r\n        <sidebar-info\r\n          v-if=\"data.name\"\r\n          :name=\"data.name\"\r\n          :feedType=\"feedTypes['community']\"\r\n          :description=\"data.description\"\r\n          :links=\"data.links\"\r\n          :isEditable=\"data.userHasEditPermissions\"\r\n          :isMember=\"data.aliasIsMember\"\r\n          :isSubscribe=\"data.aliasFollowing\"\r\n          @join=\"enterCommunity\"\r\n          @leave=\"leaveCommunity\"\r\n          @subscribe=\"subscribeToCommunity\"\r\n          @unsubscribe=\"unsubscribeFromCommunity\"\r\n          @edit=\"editCommunity\"\r\n        />\r\n      </div>\r\n      <div v-if=\"data.BlogPosts\" class=\"community-main\">\r\n        <!-- @TODO revise if we need to use showButtons here (it used to hide save btns from settings tab) -->\r\n        <component\r\n          v-if=\"data && data.id\"\r\n          :current-page=\"currentPage\"\r\n          :is=\"currentComponent\"\r\n          :data=\"data\"\r\n          :rules=\"data.communityRules\"\r\n          :settings=\"settings\"\r\n          :communityId=\"data.id\"\r\n          :blogPosts=\"data.BlogPosts\"\r\n          :showButtons=\"false\"\r\n          :disabled=\"true\"\r\n          @onPageChange=\"onPageChange\"\r\n          @openModal=\"handleModalOpen\"\r\n        />\r\n      </div>\r\n      <div class=\"community-wrapper\">\r\n        <!-- @TODO revise isEditable -->\r\n        <sidebar-tags\r\n          :isEditable=\"true\"\r\n          headingText=\"Community tags\"\r\n          :tags=\"data.Tags\"\r\n          @openTagEdit=\"setTagsEdit\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <moderation-request-modal\r\n      :settings=\"moderationModalSettings\"\r\n      :data=\"moderationModalData\"\r\n    />\r\n    <members-modal\r\n      v-if=\"modalSettings.show\"\r\n      :settings=\"modalSettings\"\r\n      :communityId=\"data.id\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from 'vuex';\r\n/** @TODO add to a corresponding mixin */\r\nimport { feedTypes } from '@/services/feed.service';\r\nimport getComponentsByPrefix from '@/utils/getComponentsByPrefix';\r\nimport {\r\n  communityRoot,\r\n  joinCommunity,\r\n  removeMemberFromCommunity,\r\n  setCommunityTags\r\n} from '@/services/Community/community.service';\r\nimport {\r\n  createFollow,\r\n  deleteFollow,\r\n  followableTypes\r\n} from '@/services/follow.service';\r\nimport navTabs from '@/services/Community/community.navTabs';\r\nimport settings from '@/services/Community/community.settings';\r\nimport moderationModalData from '@/services/Community/community.moderationModalData';\r\nimport MembersModal from '@/components/Community/MembersModal';\r\nimport Avatar from '~common/Avatar';\r\nimport Tabs from '~common/Tabs';\r\nimport tabsMixin from '@/mixins/tabs.mixin';\r\nimport tagEditMixin from '@/mixins/tag-edit.mixin';\r\nimport SidebarInfo from '~common/SidebarInfo';\r\nimport SidebarTags from '~common/SidebarTags';\r\nimport Wallpaper from '~common/Wallpaper';\r\nimport BlogSettings from '@/components/Blog/BlogSettings';\r\nimport ModerationRequestModal from '@/components/Community/ModerationRequestModal';\r\nimport CommunityAllPosts from '@/components/Community/CommunityAllPosts';\r\nimport CommunityMembers from '@/components/Community/CommunityMembers';\r\nimport CommunityModerators from '@/components/Community/CommunityModerators';\r\nimport CommunityRules from '@/components/Community/CommunityRules';\r\nimport CommunityRequests from '@/components/Community/CommunityRequests';\r\nimport CommunityCreateRules from '@/components/Community/CommunityCreateRules';\r\nimport CommunitySettings from '../components/Community/CommunitySettings';\r\n\r\nexport default {\r\n  props: {\r\n    id: {\r\n      required: true\r\n    },\r\n    postId: {\r\n      type: [Number, String]\r\n    }\r\n  },\r\n  components: {\r\n    ModerationRequestModal,\r\n    Avatar,\r\n    Tabs,\r\n    SidebarTags,\r\n    BlogSettings,\r\n    SidebarInfo,\r\n    Wallpaper,\r\n    MembersModal,\r\n    // communityAreas (the order should be strict)\r\n    CommunityAllPosts,\r\n    CommunityMembers,\r\n    CommunityRules,\r\n    CommunityModerators,\r\n    CommunityRequests,\r\n    CommunitySettings,\r\n    CommunityCreateRules\r\n  },\r\n  mixins: [tabsMixin, tagEditMixin({ setTags: setCommunityTags })],\r\n  data() {\r\n    return {\r\n      currentPage: 1,\r\n      activeTab: 0,\r\n      currentComponent: null,\r\n      settings,\r\n      // @TODO we need to implement selectOptions in settings flow\r\n      navTabs,\r\n      moderationModalSettings: {\r\n        show: false,\r\n        title: 'Moderation request',\r\n        dismissText: 'Decline',\r\n        actionText: 'Accept'\r\n      },\r\n      modalSettings: {\r\n        show: false,\r\n        title: 'Invite members to ',\r\n        dismissText: 'Cancel',\r\n        actionText: '+ Add',\r\n        data: []\r\n      },\r\n      feedTypes,\r\n      /** @TODO change to real data and remove 'src\\services\\Community\\community.moderationModalData.js' */\r\n      moderationModalData\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      activeAliasId: state => state.alias.activeAliasId,\r\n      data: state => state.feed.data\r\n    }),\r\n    communityComponents() {\r\n      return getComponentsByPrefix.call(this, communityRoot);\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions(['getFeedData', 'setBlogPostId']),\r\n    getData() {\r\n      return this.getFeedData({\r\n        id: this.id,\r\n        pagination: this.currentPage,\r\n        type: feedTypes.community\r\n      }).then(() => {\r\n        this.modalSettings.title += this.data.name;\r\n      });\r\n    },\r\n    onPageChange(page) {\r\n      this.currentPage = page;\r\n      this.getData();\r\n    },\r\n    async enterCommunity() {\r\n      const payload = {\r\n        alias: this.activeAliasId\r\n      };\r\n      try {\r\n        await joinCommunity(this.data.id, payload);\r\n        this.data.aliasIsMember = true;\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    // @TODO implement in future*/\r\n    async leaveCommunity(aliasIdToRemove = this.activeAliasId) {\r\n      const payload = {\r\n        alias: this.activeAliasId,\r\n        removeMemberId: aliasIdToRemove\r\n      };\r\n      try {\r\n        await removeMemberFromCommunity(this.data.id, payload);\r\n        this.data.aliasIsMember = false;\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    async subscribeToCommunity() {\r\n      try {\r\n        await createFollow(this.data.id, followableTypes['community']);\r\n        await this.getData();\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    async unsubscribeFromCommunity() {\r\n      try {\r\n        await deleteFollow(this.data.id, followableTypes['community']);\r\n        await this.getData();\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    async editCommunity() {\r\n      await this.$router.push({\r\n        name: 'CommunityEditCommunity',\r\n        params: { id: this.data.id }\r\n      });\r\n    },\r\n    handleModalOpen() {\r\n      this.modalSettings.show = true;\r\n    },\r\n    setComponent(tabIndex = 0, componentIndex = 0) {\r\n      this.handleSetActiveTab(tabIndex);\r\n      this.currentComponent = this.communityComponents[componentIndex];\r\n    }\r\n  },\r\n  watch: {\r\n    postId(id) {\r\n      this.setBlogPostId(id);\r\n    }\r\n  },\r\n  async mounted() {\r\n    await this.getData();\r\n    this.setComponent();\r\n    if (this.postId) {\r\n      this.setBlogPostId(this.postId);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.community {\r\n  grid-area: content;\r\n  overflow: auto;\r\n  background-color: var(--BG-primary);\r\n\r\n  ::v-deep &-tabs .nav-tabs li button {\r\n    font-size: var(--size-button);\r\n    text-transform: uppercase;\r\n  }\r\n  &-header {\r\n    display: grid;\r\n    align-items: end;\r\n    grid-template-rows: minmax(0, 60px);\r\n    grid-template-columns: var(--width-small) max-content var(--width-small);\r\n    grid-gap: var(--element-margin-xl);\r\n    background-color: var(--BG-blog-post);\r\n    padding: 0 var(--element-padding-xl);\r\n    margin-bottom: var(--element-margin-xl);\r\n  }\r\n  &-content {\r\n    padding: 0 var(--element-padding-xl);\r\n    display: grid;\r\n    grid-gap: var(--element-padding-xl);\r\n    grid-template-columns: var(--width-small) auto var(--width-small);\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}