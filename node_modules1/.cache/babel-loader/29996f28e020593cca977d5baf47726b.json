A;AACA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA;AAvCA,CAAA","sourcesContent":["<template>\r\n  <div v-if=\"data.BlogPosts\" class=\"tagged-blog-posts\">\r\n    <blog-all-posts :blogPosts=\"data.BlogPosts\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from 'vuex';\r\nimport { feedTypes } from '@/services/feed.service';\r\nimport BlogAllPosts from '@/components/Blog/BlogAllPosts';\r\n\r\nexport default {\r\n  props: {\r\n    tagNames: {\r\n      required: true\r\n    },\r\n    postId: {\r\n      type: [Number, String]\r\n    }\r\n  },\r\n  components: {\r\n    BlogAllPosts\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      data: state => state.feed.data\r\n    })\r\n  },\r\n  methods: {\r\n    ...mapActions(['getFeedData', 'setBlogPostId']),\r\n    async getData() {\r\n      return this.getFeedData({\r\n        tagNames: this.tagNames,\r\n        pagination: this.currentPage,\r\n        type: feedTypes.tagged\r\n      });\r\n    }\r\n  },\r\n  /** @TODO make it common between blog and community */\r\n  watch: {\r\n    postId(id) {\r\n      this.setBlogPostId(id);\r\n    }\r\n  },\r\n  async mounted() {\r\n    await this.getData();\r\n    /** @TODO make it common between blog and community */\r\n    if (this.postId) {\r\n      this.setBlogPostId(this.postId);\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.tagged-blog-posts {\r\n  grid-area: content;\r\n  overflow: auto;\r\n  background-color: var(--BG-primary);\r\n  padding: var(--element-padding-xl);\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}