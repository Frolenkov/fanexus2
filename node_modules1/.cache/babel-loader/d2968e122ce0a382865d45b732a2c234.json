","sourcesContent":["<template>\r\n  <!-- @TODO need to revise this component for using props from outside -->\r\n  <ul class=\"action-buttons-group\">\r\n    <li class=\"action-buttons-group__item\">\r\n      <button type=\"button\" class=\"action-buttons-group__button\">\r\n        <like-normal-icon />\r\n      </button>\r\n      <span class=\"action-buttons-group__value\">{{\r\n        engagementStats.likes\r\n      }}</span>\r\n    </li>\r\n    <li class=\"action-buttons-group__item\">\r\n      <button\r\n        type=\"button\"\r\n        @click.stop=\"toggleCommentForm\"\r\n        class=\"action-buttons-group__button\"\r\n        :class=\"{ active: showNewCommentForm }\"\r\n      >\r\n        <!-- @TODO update svg output to be single image -->\r\n        <comment-normal-icon v-if=\"!showNewCommentForm\" />\r\n        <comment-active-icon v-else />\r\n      </button>\r\n      <span class=\"action-buttons-group__value\">{{\r\n        engagementStats.comments\r\n      }}</span>\r\n    </li>\r\n    <li class=\"action-buttons-group__item\">\r\n      <button type=\"button\" class=\"action-buttons-group__button\">\r\n        <reblog-normal-icon />\r\n      </button>\r\n      <span class=\"action-buttons-group__value\">{{\r\n        engagementStats.reblogs\r\n      }}</span>\r\n    </li>\r\n    <li class=\"action-buttons-group__item\">\r\n      @TODO\r\n      <button\r\n        type=\"button\"\r\n        @click.stop=\"toggleDropdown\"\r\n        class=\"action-buttons-group__button\"\r\n      >\r\n        <dots-icon />\r\n      </button>\r\n      <action-dropdown v-if=\"dropdownShow\" @close=\"handleActionDropdownClose\" />\r\n    </li>\r\n    <li v-if=\"commentsLength\" class=\"action-buttons-group__item\">\r\n      <button\r\n        type=\"button\"\r\n        @click=\"toggleChildComments\"\r\n        class=\"action-buttons-group__button\"\r\n      >\r\n        <span\r\n          class=\"action-buttons-group__text-in-button\"\r\n          v-if=\"showChildComments\"\r\n          >Hide comments ({{ commentsLength }})</span\r\n        >\r\n        <span class=\"action-buttons-group__text-in-button\" v-else\r\n          >Show comments ({{ commentsLength }})</span\r\n        >\r\n      </button>\r\n    </li>\r\n  </ul>\r\n</template>\r\n<script>\r\nimport LikeNormalIcon from '~svg/like-normal-icon.svg';\r\nimport CommentNormalIcon from '~svg/comment-normal-icon.svg';\r\nimport CommentActiveIcon from '~svg/comment-active-icon.svg';\r\nimport ReblogNormalIcon from '~svg/reblog-normal-icon.svg';\r\nimport DotsIcon from '~svg/dots-icon.svg';\r\nimport ActionDropdown from '~common/ActionDropdown';\r\n\r\nexport default {\r\n  components: {\r\n    LikeNormalIcon,\r\n    CommentNormalIcon,\r\n    CommentActiveIcon,\r\n    ReblogNormalIcon,\r\n    DotsIcon,\r\n    ActionDropdown\r\n  },\r\n  props: {\r\n    engagementStats: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    commentsLength: {\r\n      type: Number\r\n    },\r\n    showNewCommentForm: {\r\n      type: Boolean\r\n    },\r\n    showChildComments: {\r\n      type: Boolean\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      dropdownShow: false\r\n    };\r\n  },\r\n  methods: {\r\n    toggleDropdown() {\r\n      this.dropdownShow = !this.dropdownShow;\r\n    },\r\n    handleActionDropdownClose() {\r\n      this.dropdownShow = false;\r\n    },\r\n    toggleCommentForm() {\r\n      this.$emit('toggleCommentForm');\r\n    },\r\n    toggleChildComments() {\r\n      this.$emit('toggleChildComments');\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.action-buttons-group {\r\n  display: flex;\r\n  margin: 0;\r\n  margin-bottom: var(--element-margin-l);\r\n  padding: 0 var(--element-padding-l);\r\n  &.comment {\r\n    padding: 0;\r\n  }\r\n  &__item {\r\n    margin-right: var(--element-margin-l);\r\n    display: flex;\r\n    align-items: center;\r\n    position: relative;\r\n  }\r\n  &__button {\r\n    background: transparent;\r\n    margin-right: var(--element-margin-xs);\r\n    color: var(--text-button-comments-show);\r\n    font-size: var(--size-regular);\r\n    font-family: 'OpenSans-Bold';\r\n    display: flex;\r\n    &:focus {\r\n      outline: none;\r\n    }\r\n\r\n    > svg {\r\n      fill: var(--text-primary);\r\n      width: 20px;\r\n      height: 20px;\r\n    }\r\n\r\n    &.active svg {\r\n      fill: var(--BG-button-paggination-active);\r\n    }\r\n  }\r\n\r\n  &__text-in-button {\r\n    text-transform: uppercase;\r\n  }\r\n  &__value {\r\n    color: var(--text-primary);\r\n    font-size: var(--size-small);\r\n    font-family: 'OpenSans-Bold';\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/common"}]}