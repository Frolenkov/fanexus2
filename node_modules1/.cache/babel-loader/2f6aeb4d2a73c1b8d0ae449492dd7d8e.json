AA,OAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,yBAAA,IAAA,CAAA,MAAA;AAAA,iBAAA,CAAA;AAEA;;AACA,oBAAA,UAAA,CAAA,SAAA,EAAA;AACA,kBAAA,OAAA,CAAA,UAAA,GAAA,UAAA;AACA;;AAxCA;AAAA;AAAA,uBA2CA,aAAA,CAAA,KAAA,CAAA,IAAA,CAAA,EAAA,EAAA,OAAA,CA3CA;;AAAA;AAAA;AAAA,uBA4CA,KAAA,CAAA,OAAA,EA5CA;;AAAA;AA6CA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,aADA;AAEA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA,KAAA,CAAA,IAAA,CAAA;AAAA;AAFA,iBAAA;;AA7CA;AAAA;;AAAA;AAAA;AAAA;AAkDA,gBAAA,KAAA,CAAA,MAAA,GAAA,YAAA,QAAA,CAAA,IAAA,CAAA,MAAA;;AAlDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoDA,KA7DA;AA8DA,IAAA,OA9DA,qBA8DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,YAAA,CAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,IAAA,IAAA,MAAA,CAAA,IAAA,CAAA,EAAA,IAAA,MAAA,CAAA,EADA;AAEA,kBAAA,UAAA,EAAA;AAFA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,MAAA,CAAA,IAFA;;AAMA,gBAAA,MAAA,CAAA,WAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,CAAA,UAAA,GAAA;AAAA,yBAAA,GAAA,CAAA,IAAA;AAAA,iBAAA,CAAA;;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,KAAA,sBACA,MAAA,CAAA,IAAA,CAAA,cAAA,CAAA,GAAA,CAAA;AAAA,sBAAA,IAAA,SAAA,IAAA;AAAA,sBAAA,WAAA,SAAA,WAAA;AAAA,yBAAA;AACA,oBAAA,IAAA,EAAA,IADA;AAEA,oBAAA,WAAA,EAAA;AAFA,mBAAA;AAAA,iBAAA,CADA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA;AA3EA,KA4EA,UAAA,CAAA,CAAA,aAAA,EAAA,mBAAA,CAAA,CA5EA,CAvDA;AAqIA,EAAA,OArIA,qBAqIA;AACA,SAAA,YAAA;AACA,SAAA,iBAAA,CAAA,YAAA;AACA,aAAA,SAAA;AACA,KAFA;AAGA,SAAA,OAAA;AACA;AA3IA,CAAA","sourcesContent":["<template>\r\n  <div class=\"edit-community\" v-if=\"data\">\r\n    <add-image\r\n      :type=\"blogImageTypes.coverImage\"\r\n      class=\"edit-community__add-bg\"\r\n      button-text=\"Add Background\"\r\n      @add=\"handleAddImage\"\r\n    >\r\n      <wallpaper :src=\"coverImageData\" />\r\n    </add-image>\r\n    <div class=\"edit-community__header\">\r\n      <div class=\"edit-community__header-wrapper\" />\r\n      <tabs\r\n        :tabs=\"navTabs\"\r\n        @selectTab=\"$active => setComponent($active, $active)\"\r\n        :active=\"activeTab\"\r\n      />\r\n    </div>\r\n    <div class=\"edit-community__content\">\r\n      <div class=\"edit-community__content-wrapper\">\r\n        <sidebar-form\r\n          :show-buttons=\"true\"\r\n          :errors=\"errors\"\r\n          :data=\"data\"\r\n          @save=\"onSave\"\r\n        />\r\n      </div>\r\n      <div class=\"edit-community__content-main\">\r\n        <component :is=\"currentComponent\" :data=\"data\" :settings=\"settings\" />\r\n      </div>\r\n      <div class=\"edit-community__content-wrapper\">\r\n        <sidebar-tags\r\n          class=\"edit-community__content-tags\"\r\n          :isTagsEdit=\"true\"\r\n          headingText=\"Community tags\"\r\n          :tags=\"tagItems\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Wallpaper from '~common/Wallpaper';\r\nimport { mapState, mapActions } from 'vuex';\r\nimport settings from '@/services/Community/community.settings';\r\nimport AddImage from '../common/AddImage';\r\nimport Tabs from '../common/Tabs';\r\nimport tabsMixin from '@/mixins/tabs.mixin';\r\nimport SidebarTags from '../common/SidebarTags';\r\nimport SidebarForm from '../common/SidebarForm';\r\nimport CommunityCreateRules from '@/components/Community/CommunityCreateRules';\r\nimport CommunitySettings from '@/components/Community/CommunitySettings';\r\nimport {\r\n  getCommunity,\r\n  communityAreas,\r\n  editCommunity,\r\n  blogImageTypes,\r\n  communityRoot\r\n} from '@/services/Community/community.service';\r\nimport getComponentsByPrefix from '@/utils/getComponentsByPrefix';\r\nexport default {\r\n  name: 'edit-community',\r\n  components: {\r\n    SidebarForm,\r\n    AddImage,\r\n    Tabs,\r\n    CommunityCreateRules,\r\n    CommunitySettings,\r\n    SidebarTags,\r\n    Wallpaper\r\n  },\r\n  props: {\r\n    id: {\r\n      required: true\r\n    }\r\n  },\r\n  mixins: [tabsMixin],\r\n  data() {\r\n    return {\r\n      blogImageTypes,\r\n      errors: [],\r\n      data: null,\r\n      activeTab: 0,\r\n      currentComponent: null,\r\n      settings,\r\n      navTabs: [\r\n        {\r\n          title: 'The rules',\r\n          component: communityAreas.createRules\r\n        },\r\n        {\r\n          title: 'Settings',\r\n          component: communityAreas.settings\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    communityComponents() {\r\n      return getComponentsByPrefix.call(this, communityRoot);\r\n    },\r\n    /** @TODO make it common between blog and community */\r\n    coverImageData() {\r\n      if (!this.data || !this.data.coverImage) return '';\r\n\r\n      return this.data.coverImage.accessUrl || this.data.coverImage.src;\r\n    },\r\n    tagItems() {\r\n      return this.tags.map(tag => ({ name: tag, id: 0 }));\r\n    },\r\n    ...mapState({\r\n      activeAliasId: state => state.alias.activeAliasId,\r\n      tags: state => state.tags.list\r\n    })\r\n  },\r\n  methods: {\r\n    handleAddImage({ type, data }) {\r\n      this.data[type] = { ...data };\r\n    },\r\n    setComponent(tabIndex = 0, componentIndex = 0) {\r\n      this.handleSetActiveTab(tabIndex);\r\n      this.currentComponent = this.communityComponents[componentIndex];\r\n    },\r\n\r\n    async onSave() {\r\n      /** @TODO revise */\r\n      const {\r\n        name,\r\n        link,\r\n        links,\r\n        description,\r\n        coverImage,\r\n        rules,\r\n        contentAccessControl,\r\n        followsAccessControl,\r\n        commentsAccessControl,\r\n        postingAccessControl,\r\n        membersAccessControl,\r\n        reactionsAccessControl\r\n      } = this.data;\r\n      const payload = {\r\n        alias: this.activeAliasId,\r\n        contentAccessControl,\r\n        followsAccessControl,\r\n        postingAccessControl,\r\n        commentsAccessControl,\r\n        membersAccessControl,\r\n        reactionsAccessControl,\r\n        name,\r\n        link,\r\n        links,\r\n        description,\r\n        rules,\r\n        tagNames: this.tags\r\n      };\r\n      /** @TODO make it common between blog and community */\r\n      payload.rules = payload.rules.filter(\r\n        rules => rules.name.length || rules.description.length\r\n      );\r\n      payload.links = payload.links.filter(link => link.length);\r\n\r\n      /** @TODO make it common between blog and community */\r\n      if (coverImage.accessUrl) {\r\n        payload.coverImage = coverImage;\r\n      }\r\n\r\n      try {\r\n        await editCommunity(this.data.id, payload);\r\n        await this.getData();\r\n        this.$router.push({\r\n          name: communityRoot,\r\n          params: { id: this.data.link }\r\n        });\r\n      } catch (e) {\r\n        this.errors = e.response.data.errors;\r\n      }\r\n    },\r\n    async getData() {\r\n      /** @TODO revise */\r\n      this.data = await getCommunity({\r\n        id: (this.data && this.data.id) || this.id,\r\n        pagination: 1\r\n      });\r\n      this.setTagsList(this.data.Tags.map(tag => tag.name));\r\n      this.data.rules = [\r\n        ...this.data.communityRules.map(({ name, description }) => ({\r\n          name,\r\n          description\r\n        }))\r\n      ];\r\n    },\r\n    ...mapActions(['setTagsList', 'setSaveTagsMethod'])\r\n  },\r\n  mounted() {\r\n    this.setComponent();\r\n    this.setSaveTagsMethod(() => {\r\n      return undefined;\r\n    });\r\n    this.getData();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.edit-community {\r\n  grid-area: content;\r\n  overflow: auto;\r\n  background-color: var(--BG-primary);\r\n  &__add-bg {\r\n    ::v-deep .button {\r\n      padding: 8px var(--element-padding-xxl);\r\n    }\r\n  }\r\n  &__header {\r\n    display: grid;\r\n    align-items: end;\r\n    grid-template-rows: minmax(0, 60px);\r\n    grid-template-columns: var(--width-small) max-content var(--width-small);\r\n    grid-gap: var(--element-margin-xl);\r\n    background-color: var(--BG-blog-post);\r\n    padding: 0 var(--element-padding-xl);\r\n    margin-bottom: var(--element-margin-xl);\r\n  }\r\n\r\n  &__content {\r\n    padding: 0 var(--element-padding-xl);\r\n    display: grid;\r\n    grid-gap: var(--element-padding-xl);\r\n    grid-template-columns: var(--width-small) auto var(--width-small);\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Community"}]}