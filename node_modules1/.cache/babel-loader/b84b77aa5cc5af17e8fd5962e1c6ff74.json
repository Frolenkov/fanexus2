AFA;AAGA,QAAA,KAAA,EAAA,OAAA,CAAA,+BAAA;AAHA,OA/BA,EAoCA;AACA,QAAA,EAAA,EAAA,GADA;AAEA,QAAA,IAAA,EAAA,SAFA;AAGA,QAAA,KAAA,EAAA,OAAA,CAAA,+BAAA;AAHA,OApCA,EAyCA;AACA,QAAA,EAAA,EAAA,GADA;AAEA,QAAA,IAAA,EAAA,SAFA;AAGA,QAAA,KAAA,EAAA,OAAA,CAAA,+BAAA;AAHA,OAzCA,EA8CA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,UAFA;AAGA,QAAA,KAAA,EAAA,OAAA,CAAA,+BAAA;AAHA,OA9CA,EAmDA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,UAFA;AAGA,QAAA,KAAA,EAAA,OAAA,CAAA,+BAAA;AAHA,OAnDA,EAwDA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,UAFA;AAGA,QAAA,KAAA,EAAA,OAAA,CAAA,+BAAA;AAHA,OAxDA,EA6DA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,UAFA;AAGA,QAAA,KAAA,EAAA,OAAA,CAAA,+BAAA;AAHA,OA7DA,EAkEA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,UAFA;AAGA,QAAA,KAAA,EAAA,OAAA,CAAA,+BAAA;AAHA,OAlEA;AANA,KAAA;AA+EA;AAjJA,CAAA","sourcesContent":["<template>\r\n  <modal\r\n    class=\"members-modal modal-overlay--centered\"\r\n    :settings=\"settings\"\r\n    @save=\"\r\n      modalType === communityModalTypes.addMembers\r\n        ? inviteMember()\r\n        : deleteMember()\r\n    \"\r\n  >\r\n    <template slot=\"body\">\r\n      <p class=\"members-modal__desc\" v-if=\"!addedUser && !deletedUser\">\r\n        The list of people in your security group wonâ€™t be visible at your\r\n        community. Only you can see and manage it.\r\n      </p>\r\n      <p v-else-if=\"addedUser\" class=\"members-modal__user\">\r\n        User\r\n        <span class=\"members-modal__user-name\">{{ addedUser }}</span>\r\n        was successfully added to the security group.\r\n      </p>\r\n      <p v-else class=\"members-modal__user\">\r\n        User\r\n        <span class=\"members-modal__user-name\">{{ deletedUser }}</span>\r\n        was deleted from the security group.\r\n      </p>\r\n      <form\r\n        class=\"members-modal__search-field\"\r\n        v-if=\"modalType === communityModalTypes.deleteMembers\"\r\n      >\r\n        <form-input\r\n          class=\"members-modal__name\"\r\n          type=\"search\"\r\n          name=\"find-community\"\r\n          placeholder=\"Find community\"\r\n        />\r\n      </form>\r\n      <div class=\"members-modal__container\">\r\n        <div class=\"members-modal__wrapper\" slot=\"body\">\r\n          <author-title\r\n            v-for=\"item of data\"\r\n            :key=\"item.id\"\r\n            class=\"group__item\"\r\n            :name=\"item.name\"\r\n            :image=\"item.image\"\r\n            :class=\"{ active: activeItem === item.id }\"\r\n            @click.native.stop=\"activateItem(item.id)\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </template>\r\n  </modal>\r\n</template>\r\n\r\n<script>\r\nimport { deleteItemById } from '@/utils/array';\r\nimport { communityModalTypes } from '@/services/community.service';\r\nimport Modal from '../common/Modal';\r\nimport FormInput from '../common/Fields/FormInput';\r\nimport AuthorTitle from '../common/AuthorTitle';\r\nimport {\r\n  inviteMember,\r\n  removeFromCommunity\r\n} from '@/services/community.service';\r\n\r\nexport default {\r\n  components: {\r\n    AuthorTitle,\r\n    Modal,\r\n    FormInput\r\n  },\r\n  props: {\r\n    settings: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    modalType: {\r\n      type: String,\r\n      default: communityModalTypes[0],\r\n      validator: value => Object.values(communityModalTypes).includes(value)\r\n    },\r\n    communityId: {\r\n      type: Number,\r\n      required: true\r\n    }\r\n  },\r\n  methods: {\r\n    activateItem(id) {\r\n      this.activeItem = id;\r\n    },\r\n    // /*@TODO change alias after store will be done*/\r\n    async inviteMember() {\r\n      const payload = {\r\n        alias: 1,\r\n        invitedUserId: this.activeItem\r\n      };\r\n      try {\r\n        await inviteMember(this.communityId, payload);\r\n        this.updateModalData(true);\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    async deleteMember() {\r\n      const payload = {\r\n        alias: 1,\r\n        removeMemberId: this.activeItem\r\n      };\r\n      try {\r\n        await removeFromCommunity(this.communityId, payload);\r\n        this.updateModalData(false);\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    updateModalData(isAdd) {\r\n      const timeOut = 30000;\r\n      deleteItemById(this.activeItem, this.data, indexToDelete => {\r\n        const getUserName = this.data[indexToDelete].name;\r\n        this.activeItem = null;\r\n        if (isAdd) {\r\n          this.addedUser = getUserName;\r\n          setTimeout(() => (this.addedUser = null), timeOut);\r\n        } else {\r\n          this.deletedUser = getUserName;\r\n          setTimeout(() => (this.deletedUser = null), timeOut);\r\n        }\r\n      });\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      communityModalTypes,\r\n      activeItem: null,\r\n      addedUser: null,\r\n      deletedUser: null,\r\n      // @TODO remove after connecting api*/\r\n      data: [\r\n        {\r\n          id: '1',\r\n          name: 'member1',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '2',\r\n          name: 'member2',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '3',\r\n          name: 'member3',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '4',\r\n          name: 'member4',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '5',\r\n          name: 'member5',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '6',\r\n          name: 'member6',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '7',\r\n          name: 'member7',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '8',\r\n          name: 'member8',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '9',\r\n          name: 'member9',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '10',\r\n          name: 'member10',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '11',\r\n          name: 'member12',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '12',\r\n          name: 'member12',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '13',\r\n          name: 'member13',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        },\r\n        {\r\n          id: '14',\r\n          name: 'member14',\r\n          image: require('@/assets/img/png/Image_29.png')\r\n        }\r\n      ]\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.members-modal {\r\n  padding: var(--element-padding-xl);\r\n  &__desc,\r\n  &__user {\r\n    font-family: OpenSans-Regular;\r\n    font-size: var(--size-regular);\r\n    color: var(--text-primary);\r\n    margin-bottom: var(--element-margin-l);\r\n    line-height: 1.5em;\r\n  }\r\n  &__user {\r\n    color: var(--text-secondary);\r\n    &-name {\r\n      font-family: 'OpenSans-Bold', sans-serif;\r\n    }\r\n  }\r\n  &__container {\r\n    max-height: 50vh;\r\n    overflow: auto;\r\n  }\r\n  &__wrapper {\r\n    display: grid;\r\n    grid-gap: var(--element-margin-s);\r\n    grid-template-columns: repeat(2, 1fr);\r\n    padding-right: var(--element-padding-s);\r\n    padding-bottom: var(--element-padding-s);\r\n  }\r\n\r\n  &__search-field {\r\n    position: relative;\r\n    align-items: center;\r\n    margin-bottom: var(--element-margin-l);\r\n    background-color: var(--BG-input);\r\n  }\r\n  ::v-deep {\r\n    .modal-body {\r\n      padding-bottom: 0;\r\n    }\r\n    .group__item {\r\n      padding: var(--element-padding-m);\r\n      margin-bottom: 0;\r\n      border: var(--border-width-thin) solid var(--border-group-normal);\r\n      border-radius: var(--border-radius);\r\n      cursor: pointer;\r\n      background-color: var(--BG-input);\r\n      &:hover {\r\n        border-color: var(--border-input-hover);\r\n      }\r\n      &.active {\r\n        border-color: var(--border-active);\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Community"}]}