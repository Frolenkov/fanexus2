A;AACA,UAAA,oBAAA,EAAA;AADA,SArBA;AAwBA,QAAA,oBAAA,EAAA;AACA,UAAA,oBAAA,EAAA;AADA;AAxBA,OAFA;AA8BA,MAAA,SAAA,EAAA,CA9BA;AA+BA,MAAA,gBAAA,EAAA,IA/BA;AAgCA,MAAA,QAAA,EAAA,QAhCA;AAiCA,MAAA,OAAA,EAAA,CACA;AACA,QAAA,KAAA,EAAA,WADA;AAEA,QAAA,SAAA,EAAA,cAAA,CAAA;AAFA,OADA,EAKA;AACA,QAAA,KAAA,EAAA,UADA;AAEA,QAAA,SAAA,EAAA,cAAA,CAAA;AAFA,OALA;AAjCA,KAAA;AA4CA,GAvDA;AAwDA,EAAA,QAAA,EAAA;AACA,IAAA,mBADA,iCACA;AACA,aAAA,qBAAA,CAAA,IAAA,CAAA,IAAA,EAAA,aAAA,CAAA;AACA;AAHA,GAxDA;AA6DA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AAAA,UAAA,QAAA,uEAAA,CAAA;AAAA,UAAA,cAAA,uEAAA,CAAA;AACA,WAAA,kBAAA,CAAA,QAAA;AACA,WAAA,gBAAA,GAAA,KAAA,mBAAA,CAAA,cAAA,CAAA;AACA,KAJA;AAKA,IAAA,MALA,oBAKA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA;AACA,gBAAA,KAFA,GAEA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CACA,UAAA,IAAA;AAAA,uCAAA,IAAA,CAAA,QAAA,sBAAA,IAAA,CAAA,eAAA;AAAA,iBADA,CAFA;AAKA,gBAAA,OALA,qBAMA,KAAA,CAAA,QANA;AAOA,kBAAA,KAAA,EAAA,CAPA;AAQA,kBAAA,KAAA,EAAA,KARA;AASA,kBAAA,MAAA,EAAA;AACA,oBAAA,IAAA,EAAA,QADA;AAEA,oBAAA,GAAA,EACA,oIAHA;AAIA,oBAAA,GAAA,EAAA;AAJA,mBATA;AAeA,kBAAA,UAAA,EAAA;AACA,oBAAA,IAAA,EAAA,UADA;AAEA,oBAAA,GAAA,EACA,4EAHA;AAIA,oBAAA,GAAA,EAAA;AAJA,mBAfA;AAqBA,kBAAA,oBAAA,EAAA,MArBA;AAsBA,kBAAA,IAAA,EAAA,CAAA,CAAA,EAAA,CAAA,CAtBA;AAuBA,kBAAA,UAAA,EAAA,CAAA,CAAA,CAvBA;AAwBA,kBAAA,OAAA,EAAA,CAAA,CAAA;AAxBA;AAAA;AAAA,uBA0BA,eAAA,CAAA,OAAA,CA1BA;;AAAA;AA0BA,gBAAA,KAAA,CAAA,IA1BA;;AAAA,qBA2BA,KAAA,CAAA,IAAA,CAAA,MA3BA;AAAA;AAAA;AAAA;;AA4BA,gBAAA,KAAA,CAAA,MAAA,GAAA,KAAA,CAAA,IAAA,CAAA,MAAA;AA5BA;AAAA;;AAAA;AAAA,qBA6BA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,EAAA,MA7BA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBA8BA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,WADA;AAEA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA,KAAA,CAAA,IAAA,CAAA;AAAA;AAFA,iBAAA,CA9BA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmCA;AAxCA,GA7DA;AAuGA,EAAA,OAvGA,qBAuGA;AACA,SAAA,YAAA;AACA;AAzGA,CAAA","sourcesContent":["<template>\r\n  <div class=\"create-community\">\r\n    <add-image class=\"create-community__add-bg\" button-text=\"Add Background\" />\r\n    <div class=\"create-community__header\">\r\n      <div class=\"create-community__header-wrapper\" />\r\n      <tabs\r\n        :tabs=\"navTabs\"\r\n        @selectTab=\"$active => setComponent($active, $active)\"\r\n        :active=\"activeTab\"\r\n      />\r\n    </div>\r\n    <div class=\"create-community__content\">\r\n      <div class=\"create-community__content-wrapper\">\r\n        <sidebar-form\r\n          :errors=\"errors\"\r\n          :show-buttons=\"true\"\r\n          :formData=\"formData\"\r\n          @save=\"onSave\"\r\n        />\r\n      </div>\r\n      <div class=\"create-community__content-main\">\r\n        <!-- @TODO revise usage of formData vs data here  */ -->\r\n        <keep-alive>\r\n          <component\r\n            :is=\"currentComponent\"\r\n            :showButtons=\"false\"\r\n            :formData=\"formData\"\r\n            :data=\"formData\"\r\n            :settings=\"settings\"\r\n        /></keep-alive>\r\n      </div>\r\n      <div class=\"create-community__content-wrapper\">\r\n        <sidebar-tags\r\n          class=\"create-community__content-tags\"\r\n          createText=\"Tags of your posts will be shown here. You may add them, but others will see them only when you use them in your posts.\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport settings from '@/services/Community/community.settings';\r\nimport AddImage from '../common/AddImage';\r\nimport Tabs from '../common/Tabs';\r\nimport tabsMixin from '@/mixins/tabs.mixin';\r\nimport SidebarTags from '../common/SidebarTags';\r\nimport SidebarForm from '../common/SidebarForm';\r\nimport CommunityCreateRules from '@/components/Community/CommunityCreateRules';\r\nimport CommunitySettings from '@/components/Community/CommunitySettings';\r\nimport {\r\n  communityAreas,\r\n  createCommunity,\r\n  communityRoot\r\n} from '@/services/community.service';\r\nimport getComponentsByPrefix from '@/utils/getComponentsByPrefix';\r\n\r\nexport default {\r\n  components: {\r\n    SidebarForm,\r\n    AddImage,\r\n    Tabs,\r\n    CommunityCreateRules,\r\n    CommunitySettings,\r\n    SidebarTags\r\n  },\r\n  mixins: [tabsMixin],\r\n  data() {\r\n    return {\r\n      errors: [],\r\n      formData: {\r\n        links: [],\r\n        rules: [\r\n          {\r\n            ruleName: '',\r\n            ruleDescription: ''\r\n          }\r\n        ],\r\n        /** @TODO revise structure */\r\n        followsAccessControl: {\r\n          accessControlSetting: 3\r\n        },\r\n        contentAccessControl: {\r\n          accessControlSetting: 3\r\n        },\r\n        reactionsAccessControl: {\r\n          accessControlSetting: 3\r\n        },\r\n        commentsAccessControl: {\r\n          accessControlSetting: 3\r\n        },\r\n        membersAccessControl: {\r\n          accessControlSetting: 3\r\n        },\r\n        postingAccessControl: {\r\n          accessControlSetting: 3\r\n        }\r\n      },\r\n      activeTab: 0,\r\n      currentComponent: null,\r\n      settings,\r\n      navTabs: [\r\n        {\r\n          title: 'The rules',\r\n          component: communityAreas.createRules\r\n        },\r\n        {\r\n          title: 'Settings',\r\n          component: communityAreas.settings\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    communityComponents() {\r\n      return getComponentsByPrefix.call(this, communityRoot);\r\n    }\r\n  },\r\n  methods: {\r\n    setComponent(tabIndex = 0, componentIndex = 0) {\r\n      this.handleSetActiveTab(tabIndex);\r\n      this.currentComponent = this.communityComponents[componentIndex];\r\n    },\r\n    async onSave() {\r\n      /** @TODO revise */\r\n      const rules = this.formData.rules.map(\r\n        rule => `<h3>${rule.ruleName}</h3><br>${rule.ruleDescription}`\r\n      );\r\n      const payload = {\r\n        ...this.formData,\r\n        alias: 1,\r\n        rules: rules,\r\n        avatar: {\r\n          name: 'batman',\r\n          src:\r\n            'https://upload.wikimedia.org/wikipedia/en/thumb/7/7b/Batman_%28DC_Rebirth_version%29.png/220px-Batman_%28DC_Rebirth_version%29.png',\r\n          alt: 'batsuit'\r\n        },\r\n        coverImage: {\r\n          name: 'nanosuit',\r\n          src:\r\n            'https://vignette.wikia.nocookie.net/crysis/images/f/fb/Crynet_nanosuit.jpg',\r\n          alt: 'awesomesuit'\r\n        },\r\n        subscriptionApproval: 'open',\r\n        tags: [1, 2],\r\n        moderators: [1],\r\n        members: [1]\r\n      };\r\n      this.data = await createCommunity(payload);\r\n      if (this.data.errors) {\r\n        this.errors = this.data.errors;\r\n      } else if (Object.keys(this.data).length) {\r\n        await this.$router.push({\r\n          name: 'community',\r\n          params: { id: this.data.id }\r\n        });\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.setComponent();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.create-community {\r\n  grid-area: content;\r\n  overflow: auto;\r\n  background-color: var(--BG-primary);\r\n  &__add-bg {\r\n    ::v-deep .button {\r\n      padding: 8px var(--element-padding-xxl);\r\n    }\r\n  }\r\n  &__header {\r\n    display: grid;\r\n    align-items: end;\r\n    grid-template-rows: minmax(0, 60px);\r\n    grid-template-columns: var(--width-small) max-content var(--width-small);\r\n    grid-gap: var(--element-margin-xl);\r\n    background-color: var(--BG-blog-post);\r\n    padding: 0 var(--element-padding-xl);\r\n    margin-bottom: var(--element-margin-xl);\r\n  }\r\n\r\n  &__content {\r\n    padding: 0 var(--element-padding-xl);\r\n    display: grid;\r\n    grid-gap: var(--element-padding-xl);\r\n    grid-template-columns: var(--width-small) auto var(--width-small);\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Community"}]}