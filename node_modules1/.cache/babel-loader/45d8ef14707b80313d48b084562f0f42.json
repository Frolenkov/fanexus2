ame: 'isDefault',\r\n    type: 'toggler',\r\n    title: 'Is default'\r\n  },\r\n  {\r\n    name: 'accessControlSetting',\r\n    type: 'radio-button',\r\n    title: 'Access control setting',\r\n    selections: [\r\n      {\r\n        description: 'Full permissions (allow everyone)',\r\n        value: 1\r\n      },\r\n      {\r\n        description: 'Subscribers (allow subscribers)',\r\n        value: 2\r\n      },\r\n      {\r\n        description: 'Allowlist',\r\n        value: 3\r\n      },\r\n      {\r\n        description: 'Blocklist',\r\n        value: 4\r\n      }\r\n    ]\r\n  }\r\n];\r\n"]}]}