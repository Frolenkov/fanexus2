e.service';\r\n\r\nconst state = {\r\n  userData: getUserData(),\r\n  isAuthenticated: hasToken()\r\n};\r\n\r\nconst actions = {\r\n  async login({ commit }, payload) {\r\n    try {\r\n      const res = await login(payload);\r\n      commit('login', res);\r\n      commit('updateAuth', res && res.userData);\r\n    } catch (e) {\r\n      return false;\r\n    }\r\n  },\r\n\r\n  async logout({ commit }) {\r\n    try {\r\n      await logout();\r\n      commit('logout');\r\n      commit('updateAuth');\r\n    } catch (e) {\r\n      return false;\r\n    }\r\n  },\r\n\r\n  async logoutAll({ commit }) {\r\n    try {\r\n      await logoutAll();\r\n      commit('logoutAll');\r\n      commit('updateAuth');\r\n    } catch (e) {\r\n      return false;\r\n    }\r\n  },\r\n\r\n  async createUser(undefined, payload) {\r\n    try {\r\n      await createUser(payload);\r\n    } catch (e) {\r\n      return false;\r\n    }\r\n  },\r\n\r\n  async getCurrentUser() {\r\n    try {\r\n      await getCurrentUser();\r\n    } catch (e) {\r\n      return false;\r\n    }\r\n  }\r\n};\r\n\r\nconst mutations = {\r\n  updateAuth(state, userData) {\r\n    state.isAuthenticated = hasToken();\r\n    userData ? setUserData(userData) : removeUserData();\r\n\r\n    state.userData = getUserData();\r\n    setApiHeader();\r\n  },\r\n  login(state, payload) {\r\n    if (payload) {\r\n      saveToken(payload.token);\r\n    }\r\n  },\r\n  logout() {\r\n    destroyToken();\r\n  },\r\n  logoutAll() {\r\n    /*@TODO revise*/\r\n    destroyToken();\r\n  }\r\n};\r\n\r\nexport default {\r\n  state,\r\n  actions,\r\n  mutations\r\n};\r\n"]}]}