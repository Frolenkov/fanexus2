AAA,CAHA;;AAAA,qBAIA,SAJA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAIA,UAAA,MAAA,SAAA,IAAA,CAJA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAIA,QAAA,MAAA,SAAA,IAAA,CAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KATA;;AAUA;AACA,IAAA,aAXA,yBAWA,aAXA,EAWA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,OADA,GACA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,aADA;AAEA,kBAAA,OAAA,EAAA,MAAA,CAAA;AAFA,iBADA;;AAKA,oBAAA,aAAA,EAAA;AACA,kBAAA,OAAA,CAAA,aAAA,GAAA,aAAA;AACA,iBAFA,MAEA;AACA,kBAAA,OAAA,CAAA,QAAA,GAAA,MAAA,CAAA,IAAA,CAAA,EAAA;AACA;;AATA;AAAA;AAAA,uBAWA,cAAA,CAAA,OAAA,CAXA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA,kDAaA,KAbA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA;AA1BA,GA1BA;AAsDA,EAAA,QAAA,oBACA,QAAA,CAAA;AACA,IAAA,QAAA,EAAA,kBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AAAA,KADA;AAEA,IAAA,aAAA,EAAA,uBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,KAAA,CAAA,aAAA;AAAA;AAFA,GAAA,CADA;AAtDA,CAAA","sourcesContent":["<template>\r\n  <article class=\"blog-post\" @click.stop=\"openBlogModal\">\r\n    <header class=\"blog-post-header\" slot=\"header\">\r\n      <author-title\r\n        :name=\"data.Alias.name\"\r\n        :image=\"data.Alias.avatar.src\"\r\n        :date=\"data.updatedAt\"\r\n        :alt=\"data.Alias.avatar.alt\"\r\n      />\r\n      <tags-list\r\n        v-if=\"data && data.Tags\"\r\n        :list=\"data.Tags\"\r\n        @click.native.stop\r\n      />\r\n    </header>\r\n    <div class=\"blog-post-body\">\r\n      <img\r\n        class=\"blog-post-body__image\"\r\n        v-if=\"data && data.image\"\r\n        :src=\"data.image.src\"\r\n        :alt=\"data.image.name\"\r\n      />\r\n      <p class=\"blog-post-body__content\" @click.stop v-html=\"data.content\" />\r\n    </div>\r\n    <action-buttons-group\r\n      v-if=\"showActionButtons\"\r\n      :engagementStats=\"data.engagementStats\"\r\n      @click.native.stop\r\n      :userHasEditPermissions=\"data.userHasEditPermissions\"\r\n      @likeClicked=\"postLikeToggle\"\r\n      :userLiked=\"data.userLiked\"\r\n    />\r\n    <comments\r\n      slot=\"footer\"\r\n      v-if=\"showComments\"\r\n      :comments=\"data.Comments\"\r\n      :newComment=\"newComment\"\r\n      @onNewCommentInput=\"comment => (newComment = comment)\"\r\n      @createComment=\"createComment\"\r\n    />\r\n  </article>\r\n</template>\r\n\r\n<script>\r\n/*@TODO revise name since src\\views\\BlogPost.vue has the same*/\r\nimport Comments from '@/components/common/Comments/Comments';\r\nimport AuthorTitle from '~common/AuthorTitle';\r\nimport TagsList from '~common/TagsList';\r\nimport ActionButtonsGroup from '~common/ActionButtonsGroup';\r\nimport { createComment } from '@/services/comment.service';\r\nimport { mapState } from 'vuex';\r\nimport { postLike, postUnlike } from '@/services/blog.service';\r\n\r\nexport default {\r\n  props: {\r\n    data: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    showComments: {\r\n      type: Boolean,\r\n      default: true\r\n    },\r\n    showActionButtons: {\r\n      type: Boolean,\r\n      default: true\r\n    }\r\n  },\r\n  components: {\r\n    AuthorTitle,\r\n    TagsList,\r\n    ActionButtonsGroup,\r\n    Comments\r\n  },\r\n  data() {\r\n    return {\r\n      newComment: ''\r\n    };\r\n  },\r\n  methods: {\r\n    openBlogModal() {\r\n      this.$emit('openModal', this.data.id);\r\n    },\r\n    async postLikeToggle() {\r\n      const { id, userLiked } = this.data;\r\n      /** @TODO revise */\r\n      const body = [id, { alias: this.activeAliasId }];\r\n      userLiked ? await postUnlike(...body) : await postLike(...body);\r\n    },\r\n    /*@TODO make it dynamic and revise do we need comment id*/\r\n    async createComment(parentComment) {\r\n      const payload = {\r\n        alias: this.activeAliasId,\r\n        content: this.newComment\r\n      };\r\n      if (parentComment) {\r\n        payload.parentComment = parentComment;\r\n      } else {\r\n        payload.blogPost = this.data.id;\r\n      }\r\n      try {\r\n        await createComment(payload);\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      userData: state => state.auth.userData,\r\n      activeAliasId: state => state.alias.activeAliasId\r\n    })\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.feed-news .blog-post {\r\n  &:hover {\r\n    box-shadow: var(--box-shadow);\r\n    cursor: pointer;\r\n  }\r\n}\r\n.blog-post {\r\n  background-color: var(--BG-blog-post);\r\n  border-radius: var(--border-radius);\r\n  transition: var(--transition-default);\r\n  &-header {\r\n    margin: 0 var(--element-margin-l);\r\n    padding-top: var(--element-padding-l);\r\n  }\r\n  &-body {\r\n    &__image {\r\n      width: 100%;\r\n      height: auto;\r\n      object-fit: cover;\r\n    }\r\n    &__content {\r\n      color: var(--text-primary);\r\n      font-size: var(--size-regular);\r\n      line-height: 1.3em;\r\n      margin: var(--element-margin-l);\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Blog"}]}