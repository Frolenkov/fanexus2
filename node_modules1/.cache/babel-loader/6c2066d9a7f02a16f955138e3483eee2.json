AAA,CAAA,IAAA,GAAA,IAAA;AACA;AAXA,GAvBA;AAoCA,EAAA,OApCA,qBAoCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,OAAA,EADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA;AAtCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"feed pinterest\">\r\n    <div v-if=\"data\" class=\"feed-news \">\r\n      <blog-post-modal\r\n        :settings=\"blogModalSettings\"\r\n        v-if=\"modalBlogId\"\r\n        :id=\"modalBlogId\"\r\n      />\r\n      <!-- @TODO pass userHasEditPermissions to component -->\r\n      <blog-post\r\n        :key=\"blog.id\"\r\n        :data=\"blog\"\r\n        :showComments=\"false\"\r\n        v-for=\"blog in data.blogPosts\"\r\n        @openModal=\"openBlogModal\"\r\n      />\r\n    </div>\r\n    <div v-if=\"data\" class=\"feed-popular-blogs\">\r\n      <h3 class=\"feed-popular-blogs__title\">popular communities</h3>\r\n      <!-- @TODO switch key to blogInfo.id (currently there's an issue on BE so the ids are duplicated) -->\r\n      <popular-blog\r\n        :key=\"index\"\r\n        v-for=\"(blogInfo, index) in data.suggestions\"\r\n        :blogInfo=\"blogInfo\"\r\n      />\r\n    </div>\r\n    <footer>\r\n      <pagination />\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defaultPage, getNewsFeed } from '@/services/feed.service';\r\nimport BlogPost from '@/components/Blog/BlogPost';\r\nimport BlogPostModal from '@/components/Blog/BlogPostModal';\r\nimport Pagination from '../components/common/Pagination';\r\nimport PopularBlog from '~common/PopularBlog';\r\n\r\nexport default {\r\n  props: {\r\n    id: {\r\n      type: Number,\r\n      default: defaultPage\r\n    }\r\n  },\r\n  components: {\r\n    PopularBlog,\r\n    BlogPost,\r\n    BlogPostModal,\r\n    Pagination\r\n  },\r\n  data() {\r\n    return {\r\n      data: null,\r\n      modalBlogId: null,\r\n      blogModalSettings: {\r\n        hideHeader: true,\r\n        show: false\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    async getData() {\r\n      try {\r\n        this.data = await getNewsFeed(this.id);\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    openBlogModal(id) {\r\n      this.modalBlogId = id;\r\n      this.blogModalSettings.show = true;\r\n    }\r\n  },\r\n  async mounted() {\r\n    await this.getData();\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.feed {\r\n  overflow-y: auto;\r\n  grid-area: content;\r\n  padding: var(--element-padding-xl);\r\n  display: grid;\r\n  grid-template-columns: 65% 15%;\r\n  grid-column-gap: var(--element-padding-xl);\r\n\r\n  &.pinterest {\r\n    grid-template-columns: 85% 15%;\r\n    padding: var(--element-padding-l);\r\n    grid-column-gap: var(--element-padding-m);\r\n  }\r\n\r\n  &-popular-blogs {\r\n    padding-left: var(--element-padding-m);\r\n\r\n    &__title {\r\n      color: var(--text-primary);\r\n      text-transform: uppercase;\r\n      line-height: var(--size-button);\r\n      padding-top: var(--element-padding-xs);\r\n      padding-bottom: var(--element-padding-m);\r\n      margin-bottom: var(--element-margin-m);\r\n      font-family: 'OpenSans-Bold', sans-serif;\r\n      font-size: var(--size-button);\r\n      border-bottom: var(--border-width-thin) solid var(--popular-blog-divider);\r\n    }\r\n  }\r\n  &-news {\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr 1fr 1fr;\r\n    grid-gap: var(--element-margin-m);\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}