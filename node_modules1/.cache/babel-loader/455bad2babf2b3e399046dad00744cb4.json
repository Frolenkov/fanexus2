ALA,CAAA;AAOA;AAxBA,IAZA;AAsCA,EAAA,QAAA,oBACA,QAAA,CAAA;AACA,IAAA,eAAA,EAAA,yBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AAAA,KADA;AAEA,IAAA,WAAA,EAAA,qBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,KAAA,CAAA,WAAA;AAAA,KAFA;AAGA,IAAA,aAAA,EAAA,uBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,KAAA,CAAA,aAAA;AAAA;AAHA,GAAA,CADA,MAMA,UAAA,CAAA,CAAA,iBAAA,CAAA,CANA;AAOA,IAAA,aAPA,2BAOA;AAAA,UACA,IADA,GACA,KAAA,MADA,CACA,IADA;AAEA,aAAA,KAAA,eAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA;AACA;AAVA,IAtCA;AAkDA,EAAA,OAlDA,qBAkDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBACA,QAAA,EADA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,qBAGA,KAAA,CAAA,cAAA,EAHA;;AAAA;AAIA,cAAA,KAAA,CAAA,WAAA,CAAA,IAAA;;AAJA;AAAA;;AAAA;AAAA;AAAA;;AAAA,oBAMA,YAAA,QAAA,CAAA,MAAA,KAAA,GANA;AAAA;AAAA;AAAA;;AAAA;AAAA,qBAOA,KAAA,CAAA,MAAA,EAPA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AA7DA,CAAA","sourcesContent":["<template>\r\n  <header class=\"header\">\r\n    <div class=\"header-left\">\r\n      <router-link class=\"header__home-link\" :to=\"{ name: 'home' }\"\r\n        ><fanexus-login-logo class=\"fanexus-logo\"\r\n      /></router-link>\r\n      <nav class=\"header-navigation\">\r\n        <ul class=\"header-navigation__list\">\r\n          <li>\r\n            <router-link class=\"header-navigation__link\" :to=\"{ name: 'home' }\"\r\n              >Feed</router-link\r\n            >\r\n          </li>\r\n          <li v-if=\"activeAliasBlog && activeAliasBlog.id\">\r\n            <router-link\r\n              class=\"header-navigation__link\"\r\n              :to=\"{\r\n                name: 'blog',\r\n                params: { name: activeAliasBlog.link || activeAliasBlog.id }\r\n              }\"\r\n              >Blog</router-link\r\n            >\r\n          </li>\r\n          <li>\r\n            <router-link\r\n              class=\"header-navigation__link\"\r\n              :to=\"{ name: 'community', params: { id: 1 } }\"\r\n              >Community</router-link\r\n            >\r\n          </li>\r\n          <li>\r\n            <div class=\"header-navigation__tags\">\r\n              <tag-input\r\n                class=\"header-navigation__tags-field\"\r\n                name=\"tags\"\r\n                placeholder=\"Find by tags\"\r\n                :list=\"tags\"\r\n                @listChange=\"updateTags\"\r\n              />\r\n              <form-button\r\n                class=\"header-navigation__tags-find-button\"\r\n                @click.native.stop=\"goToTagFindPage\"\r\n                text=\"Go\"\r\n              />\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n    </div>\r\n    <div class=\"header-right\">\r\n      <form-button\r\n        v-if=\"canCreatePost\"\r\n        :header-button=\"true\"\r\n        @click.native.stop=\"openCreatePostModal\"\r\n        text=\"Create post\"\r\n      />\r\n      <template v-if=\"!isAuthenticated\">\r\n        <form-button\r\n          :header-button=\"true\"\r\n          @click.native.stop=\"openAuthModal({ tabIndex: 1, componentIndex: 1 })\"\r\n          text=\"Login\"\r\n        />\r\n        <form-button\r\n          :header-button=\"true\"\r\n          @click.native.stop=\"openAuthModal\"\r\n          text=\"Create account\"\r\n          type=\"submit\"\r\n        />\r\n      </template>\r\n    </div>\r\n  </header>\r\n</template>\r\n<script>\r\nimport { routerSecureNavigation } from '@/services/router.service';\r\nimport FanexusLoginLogo from '~svg/fanexus-login-logo.svg';\r\nimport FormButton from '~common/Buttons/FormButton';\r\nimport TagInput from '~common/Fields/TagInput';\r\nimport { mapActions, mapState, mapGetters } from 'vuex';\r\nimport { hasToken } from '@/services/jwt.service';\r\n\r\nexport default {\r\n  name: 'fanexus-header',\r\n  components: {\r\n    FanexusLoginLogo,\r\n    FormButton,\r\n    TagInput\r\n  },\r\n  data() {\r\n    return {\r\n      tags: []\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions([\r\n      'getUserAliases',\r\n      'setLoggedIn',\r\n      'initActiveAlias',\r\n      'logout'\r\n    ]),\r\n    openAuthModal({ tabIndex = 0, componentIndex = 0 }) {\r\n      this.$emit('openAuthModal', { tabIndex, componentIndex });\r\n    },\r\n    openCreatePostModal() {\r\n      this.$emit('openCreatePostModal');\r\n    },\r\n    updateTags(tags) {\r\n      this.tags = tags;\r\n    },\r\n    goToTagFindPage() {\r\n      routerSecureNavigation(\r\n        {\r\n          name: 'tagged-blog-posts',\r\n          params: { tagNames: JSON.stringify(this.tags) }\r\n        },\r\n        this.tags\r\n      );\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      isAuthenticated: state => state.auth.isAuthenticated,\r\n      userAliases: state => state.alias.userAliases,\r\n      activeAliasId: state => state.alias.activeAliasId\r\n    }),\r\n    ...mapGetters(['activeAliasBlog']),\r\n    canCreatePost() {\r\n      const { meta } = this.$route;\r\n      return this.isAuthenticated && !!meta.createPost;\r\n    }\r\n  },\r\n  async mounted() {\r\n    if (hasToken()) {\r\n      try {\r\n        await this.getUserAliases();\r\n        this.setLoggedIn(true);\r\n      } catch (e) {\r\n        if (e.response.status === 401) {\r\n          await this.logout();\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.fanexus-logo {\r\n  /*@TODO fix issue when the modal is open*/\r\n  fill: var(--color-white);\r\n  height: 15px;\r\n  margin-right: calc(var(--element-margin-xxl) * 2);\r\n}\r\n.aside-menu.toggled + .header {\r\n  margin-left: -200px;\r\n}\r\n.header {\r\n  grid-area: header;\r\n  background-color: var(--BG-navbar);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  transition: var(--transition-default);\r\n  padding: 0 var(--element-padding-xl) 0 calc(var(--element-padding-xl) * 2);\r\n\r\n  &__home-link {\r\n    z-index: var(--z-index-1);\r\n  }\r\n  &-left {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  &-right {\r\n    display: flex;\r\n  }\r\n  &-navigation {\r\n    display: flex;\r\n    &__list {\r\n      display: flex;\r\n      align-items: center;\r\n      margin: 0;\r\n      list-style: none;\r\n    }\r\n    &__tags {\r\n      display: flex;\r\n      align-items: center;\r\n      &-field {\r\n        padding: 0 var(--element-padding-xs);\r\n      }\r\n      &-find-button {\r\n        margin-left: var(--element-margin-xs);\r\n      }\r\n    }\r\n\r\n    ::v-deep &__link {\r\n      color: var(--color-white);\r\n      font-size: var(--size-button);\r\n      font-weight: 600;\r\n      text-transform: uppercase;\r\n      margin-right: var(--element-margin-xxl);\r\n      text-decoration: none;\r\n      &:focus {\r\n        color: var(--color-main-600);\r\n      }\r\n\r\n      &.router-link-exact-active {\r\n        color: var(--border-tag);\r\n      }\r\n    }\r\n  }\r\n}\r\n.wrapper.logged .header {\r\n  padding: 0 calc(var(--element-padding-xl) * 2);\r\n  min-height: 60px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Layout"}]}