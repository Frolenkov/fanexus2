EADA;AAEA,MAAA,iBAAA,EAAA,KAFA;AAGA,MAAA,kBAAA,EAAA,KAHA;AAIA,MAAA,sBAAA,EAAA,sBAJA;AAKA,MAAA,OAAA,EAAA,KALA;AAMA,MAAA,aAAA,EAAA;AANA,KAAA;AAQA,GAxCA;AAyCA,EAAA,OAAA,oBACA,UAAA,CAAA,CAAA,oBAAA,CAAA,CADA;AAEA;AACA,IAAA,OAHA,qBAGA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,kBAAA,CAAA,KAAA,CAAA,UAAA,CADA;;AAAA;AAEA,gBAAA,KAAA,CAAA,kBAAA,GAAA,KAAA;AACA,gBAAA,KAAA,CAAA,UAAA,GAAA,EAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAPA;;AAQA;AACA,IAAA,mBATA,+BASA,eATA,EASA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oBACA,MAAA,CAAA,UADA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAGA,gBAAA,OAHA,GAGA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,aADA;AAEA,kBAAA,OAAA,EAAA,MAAA,CAAA;AAFA,iBAHA;;AAQA,oBAAA,eAAA,EAAA;AACA,kBAAA,OAAA,CAAA,aAAA,GAAA,eAAA;AACA,iBAFA,MAEA;AACA,kBAAA,OAAA,CAAA,QAAA,GAAA,MAAA,CAAA,UAAA;AACA;;AAZA;AAAA;AAAA,uBAeA,aAAA,CAAA,OAAA,CAfA;;AAAA;AAAA;AAAA,uBAgBA,MAAA,CAAA,OAAA,EAhBA;;AAAA;AAiBA,gBAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;AAjBA;AAAA;;AAAA;AAAA;AAAA;AAAA,kDAmBA,KAnBA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA,KA9BA;AA+BA,IAAA,mBA/BA,+BA+BA,EA/BA,EA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,aAAA,CAAA,EAAA,EAAA;AAAA,kBAAA,KAAA,EAAA,MAAA,CAAA;AAAA,iBAAA,CADA;;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,OAAA,EAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KAlCA;AAmCA,IAAA,iBAnCA,+BAmCA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,KArCA;AAsCA,IAAA,mBAtCA,+BAsCA,EAtCA,EAsCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,OADA,GACA,CAAA,EAAA,EAAA;AAAA,kBAAA,OAAA,EAAA,MAAA,CAAA;AAAA,iBAAA,CADA;AAAA;AAAA,uBAEA,aAAA,MAAA,SAAA,OAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AAHA;AAAA,uBAIA,MAAA,CAAA,OAAA,EAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KA3CA;AA4CA,IAAA,iBA5CA,+BA4CA;AACA,UAAA,KAAA,MAAA,CAAA,MAAA,CAAA,MAAA,EAAA;AACA,aAAA,KAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,YAAA,KAAA,MAAA,CAAA,IAAA,oBADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,MAAA,EAAA,KAAA,OAAA,CAAA;AADA;AAFA,OAAA,CAAA;AAMA,KApDA;AAqDA,IAAA,iBArDA,+BAqDA;AAAA;;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,iCACA,MAAA,CAAA,OADA,EACA,EADA,kBACA,EADA,EACA,SADA,kBACA,SADA;AAEA,gBAAA,IAFA,GAEA,CAAA,EAAA,EAAA;AAAA,kBAAA,KAAA,EAAA,MAAA,CAAA;AAAA,iBAAA,CAFA;;AAAA,qBAIA,SAJA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAIA,aAAA,MAAA,SAAA,IAAA,CAJA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAIA,WAAA,MAAA,SAAA,IAAA,CAJA;;AAAA;AAAA;AAAA,uBAKA,MAAA,CAAA,OAAA,EALA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA;AA3DA;AAzCA,CAAA","sourcesContent":["<template>\r\n  <article class=\"comments-list-item\">\r\n    <div class=\"comments-list-item__wrapper\">\r\n      <avatar\r\n        class=\"comments-list-item__avatar\"\r\n        size=\"md\"\r\n        :file=\"comment.Alias.avatar.src\"\r\n      />\r\n      <div class=\"comments-list-item__content\">\r\n        <div class=\"comments-list-item-title\">\r\n          <h2 class=\"comments-list-item-title__name\">\r\n            {{ comment.Alias.name }}\r\n          </h2>\r\n          <p\r\n            class=\"comments-list-item-title__date\"\r\n            :class=\"{ 'comments-list-item-title__date-standalone': standalone }\"\r\n            @click.stop=\"openBlogPostModal\"\r\n          >\r\n            {{ comment.updatedAt | dateFormat }}\r\n          </p>\r\n        </div>\r\n        <comment-input\r\n          :new-comment=\"comment.content\"\r\n          v-if=\"editing\"\r\n          :editing=\"true\"\r\n          @input=\"comment => (editedComment = comment)\"\r\n          @createComment=\"handleUpdateComment(comment.id)\"\r\n        />\r\n        <div class=\"comments-list-item__text\" v-html=\"comment.content\" v-else />\r\n        <action-buttons-group\r\n          class=\"comment\"\r\n          :engagementStats=\"comment.engagementStats\"\r\n          :commentsLength=\"commentsLength\"\r\n          :showNewCommentForm=\"showNewCommentForm\"\r\n          :showChildComments=\"showChildComments\"\r\n          :userHasEditPermissions=\"comment.userHasEditPermissions\"\r\n          @toggleCommentForm=\"showNewCommentForm = !showNewCommentForm\"\r\n          @toggleChildComments=\"showChildComments = !showChildComments\"\r\n          @likeClicked=\"commentLikeToggle\"\r\n          @delete=\"() => handleDeleteComment(comment.id)\"\r\n          @edit=\"handleEditComment\"\r\n          :userLiked=\"comment.userLiked\"\r\n          :options=\"dropdownOptionsComment\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <new-comment-form\r\n      v-if=\"showNewCommentForm\"\r\n      class=\"child\"\r\n      @input=\"comment => (newComment = comment)\"\r\n      @createComment=\"() => handleCreateComment(comment.id)\"\r\n    />\r\n    <div\r\n      v-if=\"commentsLength && showChildComments\"\r\n      class=\"child-comment__wrapper\"\r\n    >\r\n      <comment\r\n        v-for=\"(childComment, index) in comment.childComments\"\r\n        :key=\"`${childComment.id}-${index}`\"\r\n        :comment=\"childComment\"\r\n        :blogPostId=\"blogPostId\"\r\n        @input=\"comment => (newComment = comment)\"\r\n        @deleteComment=\"() => handleDeleteComment(childComment.id)\"\r\n        @createComment=\"() => handleCreateComment(childComment.parentCommentId)\"\r\n      />\r\n    </div>\r\n  </article>\r\n</template>\r\n<script>\r\nimport { mapState, mapActions } from 'vuex';\r\nimport {\r\n  createComment,\r\n  updateComment,\r\n  commentDelete,\r\n  commentLike,\r\n  commentUnlike\r\n} from '@/services/Comment/comment.service';\r\nimport dropdownOptionsComment from '@/services/Comment/comment.dropdownOptions';\r\nimport ActionButtonsGroup from '~common/ActionButtonsGroup';\r\nimport Avatar from '~common/Avatar';\r\nimport NewCommentForm from '@/components/common/Comments/NewCommentForm';\r\nimport Comment from '@/components/common/Comments/Comment';\r\nimport CommentInput from '@/components/common/Comments/CommentInput';\r\n\r\nexport default {\r\n  name: 'comment',\r\n  props: {\r\n    blogPostId: {\r\n      type: undefined\r\n    },\r\n    comment: {\r\n      type: Object\r\n    },\r\n    standalone: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  components: {\r\n    ActionButtonsGroup,\r\n    Avatar,\r\n    NewCommentForm,\r\n    Comment,\r\n    CommentInput\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      activeAliasId: state => state.alias.activeAliasId\r\n    }),\r\n    commentsLength() {\r\n      const comments = this.comment.childComments;\r\n\r\n      return comments ? comments.length : 0;\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      newComment: '',\r\n      showChildComments: false,\r\n      showNewCommentForm: false,\r\n      dropdownOptionsComment,\r\n      editing: false,\r\n      editedComment: ''\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions(['updateBlogPostData']),\r\n    /** @TODO make it common */\r\n    async refresh() {\r\n      await this.updateBlogPostData(this.blogPostId);\r\n      this.showNewCommentForm = false;\r\n      this.newComment = '';\r\n    },\r\n    /*@TODO make it dynamic and revise do we need comment id*/\r\n    async handleCreateComment(parentCommentId) {\r\n      if (!this.newComment) return;\r\n\r\n      const payload = {\r\n        alias: this.activeAliasId,\r\n        content: this.newComment\r\n      };\r\n\r\n      if (parentCommentId) {\r\n        payload.parentComment = parentCommentId;\r\n      } else {\r\n        payload.blogPost = this.blogPostId;\r\n      }\r\n\r\n      try {\r\n        await createComment(payload);\r\n        await this.refresh();\r\n        this.showChildComments = true;\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    async handleDeleteComment(id) {\r\n      await commentDelete(id, { alias: this.activeAliasId });\r\n      await this.refresh();\r\n    },\r\n    handleEditComment() {\r\n      this.editing = true;\r\n    },\r\n    async handleUpdateComment(id) {\r\n      const payload = [id, { content: this.editedComment }];\r\n      await updateComment(...payload);\r\n      this.editing = false;\r\n      await this.refresh();\r\n    },\r\n    openBlogPostModal() {\r\n      if (this.$route.params.postId) return;\r\n      return this.$router.push({\r\n        name: `${this.$route.name}-with-blog-post`,\r\n        params: {\r\n          postId: this.comment.BlogPostId\r\n        }\r\n      });\r\n    },\r\n    async commentLikeToggle() {\r\n      const { id, userLiked } = this.comment;\r\n      const body = [id, { alias: this.activeAliasId }];\r\n\r\n      userLiked ? await commentUnlike(...body) : await commentLike(...body);\r\n      await this.refresh();\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.comments-list-item {\r\n  display: flex;\r\n  flex-direction: column;\r\n  margin-top: var(--element-margin-m);\r\n\r\n  &__wrapper {\r\n    display: flex;\r\n    margin-bottom: var(--element-margin-m);\r\n  }\r\n  &__avatar {\r\n    margin-right: var(--element-margin-m);\r\n  }\r\n  &__content {\r\n    flex-grow: 1;\r\n  }\r\n\r\n  .comment {\r\n    margin-bottom: var(--element-padding-m);\r\n  }\r\n\r\n  &-title {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-bottom: var(--element-margin-m);\r\n    &__name {\r\n      color: var(--text-primary);\r\n      font-size: var(--size-regular);\r\n      margin-bottom: 0;\r\n      margin-right: var(--element-margin-s);\r\n    }\r\n    &__date {\r\n      color: var(--text-date-and-time);\r\n      font-size: var(--size-small);\r\n      margin-bottom: 0;\r\n      &-standalone {\r\n        cursor: pointer;\r\n      }\r\n    }\r\n  }\r\n  &__text {\r\n    color: var(--text-primary);\r\n    font-size: var(--size-regular);\r\n    line-height: 1.5;\r\n    word-break: break-word;\r\n    margin-bottom: var(--element-margin-m);\r\n    ::v-deep {\r\n      ul {\r\n        margin: var(--element-margin-s);\r\n        padding-left: 40px;\r\n      }\r\n\r\n      a {\r\n        color: var(--text-secondary);\r\n      }\r\n\r\n      blockquote {\r\n        padding-left: var(--element-padding-s);\r\n        border-left: var(--border-width-default) solid\r\n          var(--border-group-normal);\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.child-comment__wrapper {\r\n  margin-left: 65px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/common/Comments"}]}