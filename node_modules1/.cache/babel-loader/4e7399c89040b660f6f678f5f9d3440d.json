A;AACA;;AACA,oBAAA,MAAA,CAAA,SAAA,EAAA;AACA,kBAAA,OAAA,CAAA,MAAA,GAAA,MAAA;AACA;;AAvCA;AAAA;AAAA,uBA0CA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,EAAA,EAAA,OAAA,CA1CA;;AAAA;AAAA;AAAA,uBA2CA,KAAA,CAAA,OAAA,EA3CA;;AAAA;AA4CA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,MAAA,EAAA;AAAA,oBAAA,IAAA,EAAA,KAAA,CAAA,IAAA,CAAA,IAAA,IAAA,KAAA,CAAA;AAAA;AAFA,iBAAA;;AA5CA;AAAA;;AAAA;AAAA;AAAA;AAiDA,gBAAA,KAAA,CAAA,MAAA,GAAA,YAAA,QAAA,CAAA,IAAA,CAAA,MAAA;;AAjDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmDA,KAvDA;AAwDA,IAAA,OAxDA,qBAwDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,WAAA,CAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,IAAA,IAAA,MAAA,CAAA,IAAA,CAAA,EAAA,IAAA,MAAA,CAAA,EADA;AAEA,kBAAA,UAAA,EAAA;AAFA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,MAAA,CAAA,IAFA;;AAMA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,CAAA,UAAA,GAAA;AAAA,yBAAA,GAAA,CAAA,IAAA;AAAA,iBAAA,CAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAhEA;AAiEA,IAAA,YAjEA,0BAiEA;AAAA,UAAA,QAAA,uEAAA,CAAA;AAAA,UAAA,cAAA,uEAAA,CAAA;AACA,WAAA,kBAAA,CAAA,QAAA;AACA,WAAA,gBAAA,GAAA,KAAA,cAAA,CAAA,cAAA,CAAA;AACA;AApEA,KAqEA,UAAA,CAAA,CAAA,aAAA,CAAA,CArEA,CA5DA;AAmIA,EAAA,OAnIA,qBAmIA;AACA,SAAA,OAAA;AACA,SAAA,YAAA;AACA;AAtIA,CAAA","sourcesContent":["<template>\r\n  <div class=\"edit-blog\" v-if=\"data\">\r\n    <add-image\r\n      :type=\"blogImageTypes.coverImage\"\r\n      class=\"edit-blog__add-bg\"\r\n      button-text=\"Add New Background\"\r\n      @add=\"handleAddImage\"\r\n    >\r\n      <wallpaper :src=\"coverImageData\" />\r\n    </add-image>\r\n    <div class=\"edit-blog__header\">\r\n      <div class=\"edit-blog__header-wrapper\">\r\n        <add-image\r\n          :type=\"blogImageTypes.avatar\"\r\n          class=\"edit-blog__header-add-img\"\r\n          @add=\"handleAddImage\"\r\n          button-text=\"Add New Image\"\r\n        >\r\n          <avatar\r\n            class=\"blog-header__avatar\"\r\n            size=\"xl\"\r\n            :file=\"avatarImageData\"\r\n          />\r\n        </add-image>\r\n      </div>\r\n      <tabs\r\n        :tabs=\"navTabs\"\r\n        @selectTab=\"$active => setComponent($active, $active)\"\r\n        :active=\"activeTab\"\r\n      />\r\n    </div>\r\n    <div class=\"edit-blog__content\">\r\n      <div class=\"edit-blog__content-wrapper\">\r\n        <sidebar-form\r\n          placeholder-name=\"Blog Name\"\r\n          placeholder-desc=\"Blog Description\"\r\n          :show-buttons=\"true\"\r\n          :data=\"data\"\r\n          :errors=\"errors\"\r\n          @save=\"onSave\"\r\n        />\r\n      </div>\r\n      <div class=\"edit-blog__content-main\">\r\n        <keep-alive>\r\n          <component\r\n            :is=\"currentComponent\"\r\n            :data=\"data\"\r\n            :settings=\"settings\"\r\n            @save=\"onSave\"\r\n            :disabled=\"false\"\r\n          />\r\n        </keep-alive>\r\n      </div>\r\n      <div class=\"edit-blog__content-wrapper\">\r\n        <sidebar-tags\r\n          class=\"edit-community__content-tags\"\r\n          :isTagsEdit=\"true\"\r\n          :tags=\"tagItems\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport FormButton from '../common/Buttons/FormButton';\r\nimport { mapState, mapActions } from 'vuex';\r\nimport AddImage from '../common/AddImage';\r\nimport Tabs from '../common/Tabs';\r\nimport tabsMixin from '@/mixins/tabs.mixin';\r\nimport SidebarForm from '../common/SidebarForm';\r\nimport SidebarTags from '../common/SidebarTags';\r\nimport BlogSettings from './BlogSettings';\r\nimport {\r\n  getBlogData,\r\n  blogAreas,\r\n  blogRoot,\r\n  blogImageTypes,\r\n  editBlog\r\n} from '@/services/Blog/blog.service';\r\nimport settings from '@/services/Blog/blog.settings';\r\nimport getComponentsByPrefix from '@/utils/getComponentsByPrefix';\r\nimport Wallpaper from '~common/Wallpaper';\r\nimport Avatar from '~common/Avatar';\r\n\r\nexport default {\r\n  name: 'edit-blog',\r\n  components: {\r\n    SidebarForm,\r\n    AddImage,\r\n    SidebarTags,\r\n    Tabs,\r\n    BlogSettings,\r\n    Wallpaper,\r\n    Avatar,\r\n    FormButton\r\n  },\r\n  props: {\r\n    id: {\r\n      type: [String, Number],\r\n      required: true\r\n    }\r\n  },\r\n  mixins: [tabsMixin],\r\n  data() {\r\n    return {\r\n      blogImageTypes,\r\n      errors: [],\r\n      settings,\r\n      navTabs: [\r\n        {\r\n          title: 'Settings',\r\n          component: blogAreas.settings\r\n        }\r\n      ],\r\n      data: null,\r\n      activeTab: 0,\r\n      currentComponent: null\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      activeAliasId: state => state.alias.activeAliasId,\r\n      tags: state => state.tags.list\r\n    }),\r\n    /** @TODO make it common between blog and community */\r\n    coverImageData() {\r\n      if (!this.data || !this.data.coverImage) return '';\r\n\r\n      return this.data.coverImage.accessUrl || this.data.coverImage.src;\r\n    },\r\n    /** @TODO make it common between blog and community */\r\n    avatarImageData() {\r\n      if (!this.data || !this.data.avatar) return '';\r\n\r\n      return this.data.avatar.accessUrl || this.data.avatar.src;\r\n    },\r\n    /** @TODO make it common between blog and community */\r\n    blogComponents() {\r\n      return getComponentsByPrefix.call(this, blogRoot);\r\n    },\r\n    tagItems() {\r\n      return this.tags.map(tag => ({ name: tag, id: 0 }));\r\n    }\r\n  },\r\n  methods: {\r\n    handleAddImage({ type, data }) {\r\n      this.data[type] = { ...data };\r\n    },\r\n    async onSave() {\r\n      const {\r\n        name,\r\n        link,\r\n        links,\r\n        contentAccessControl,\r\n        followsAccessControl,\r\n        commentsAccessControl,\r\n        reactionsAccessControl,\r\n        description,\r\n        avatar,\r\n        hideFromMinors,\r\n        hideFromSearch,\r\n        coverImage\r\n      } = this.data;\r\n      const payload = {\r\n        alias: this.activeAliasId,\r\n        name,\r\n        link,\r\n        links,\r\n        contentAccessControl,\r\n        followsAccessControl,\r\n        commentsAccessControl,\r\n        reactionsAccessControl,\r\n        hideFromMinors,\r\n        hideFromSearch,\r\n        description,\r\n        tagNames: this.tags\r\n      };\r\n\r\n      /** @TODO make it common between blog and community */\r\n      payload.links = payload.links.filter(link => link.length);\r\n\r\n      /** @TODO make it common between blog and community */\r\n      if (coverImage.accessUrl) {\r\n        payload.coverImage = coverImage;\r\n      }\r\n      if (avatar.accessUrl) {\r\n        payload.avatar = avatar;\r\n      }\r\n\r\n      try {\r\n        await editBlog(this.data.id, payload);\r\n        await this.getData();\r\n        this.$router.push({\r\n          name: blogRoot,\r\n          params: { name: this.data.link || this.id }\r\n        });\r\n      } catch (e) {\r\n        this.errors = e.response.data.errors;\r\n      }\r\n    },\r\n    async getData() {\r\n      /** @TODO revise usage of pagination. it should be done in the background */\r\n      this.data = await getBlogData({\r\n        name: (this.data && this.data.id) || this.id,\r\n        pagination: 1\r\n      });\r\n      /** @TODO make it common */\r\n      this.setTagsList(this.data.Tags.map(tag => tag.name));\r\n    },\r\n    setComponent(tabIndex = 0, componentIndex = 0) {\r\n      this.handleSetActiveTab(tabIndex);\r\n      this.currentComponent = this.blogComponents[componentIndex];\r\n    },\r\n    ...mapActions(['setTagsList'])\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n    this.setComponent();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.edit-blog {\r\n  grid-area: content;\r\n  overflow: auto;\r\n  background-color: var(--BG-primary);\r\n\r\n  &__header {\r\n    display: grid;\r\n    align-items: end;\r\n    grid-template-rows: minmax(0, 60px);\r\n    grid-template-columns: var(--width-small) max-content var(--width-small);\r\n    grid-gap: var(--element-margin-xl);\r\n    background-color: var(--BG-blog-post);\r\n    padding: 0 var(--element-padding-xl);\r\n    margin-bottom: var(--element-margin-xl);\r\n\r\n    &-wrapper {\r\n      border: var(--border-width-avatar) solid var(--color-white);\r\n      height: 200px;\r\n      z-index: 1;\r\n      border-radius: var(--border-radius);\r\n    }\r\n\r\n    &-add-img {\r\n      height: 100%;\r\n\r\n      ::v-deep .xl {\r\n        border-radius: 0;\r\n        height: 100%;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__content {\r\n    padding: 0 var(--element-padding-xl);\r\n    display: grid;\r\n    grid-gap: var(--element-padding-xl);\r\n    grid-template-columns: var(--width-small) auto var(--width-small);\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Blog"}]}