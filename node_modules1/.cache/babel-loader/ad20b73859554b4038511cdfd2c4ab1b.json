template>\r\n          </form-radio-button>\r\n          <!-- @TODO output select only if button checked -->\r\n          <form-select\r\n            :key=\"`${id}-${index}-select`\"\r\n            v-if=\"dataItem.selectOptions\"\r\n            title=\"security group name\"\r\n            :options=\"dataItem.selectOptions\"\r\n          />\r\n        </template>\r\n      </div>\r\n\r\n      <div class=\"settings-container__btns\">\r\n        <form-button\r\n          class=\"form-button transparent\"\r\n          type=\"reset\"\r\n          text=\"Cancel\"\r\n        />\r\n        <form-button class=\"form-button\" type=\"submit\" text=\"Save\" />\r\n      </div>\r\n    </div>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nimport FormRadioButton from '@/components/common/Fields/FormRadioButton';\r\nimport FormSelect from '@/components/common/Fields/FormSelect';\r\nimport FormButton from '~common/Buttons/FormButton';\r\n\r\nexport default {\r\n  components: {\r\n    FormSelect,\r\n    FormButton,\r\n    FormRadioButton\r\n  },\r\n  // @TODO  make data dynamic\r\n  data() {\r\n    return {\r\n      formData: {\r\n        subscription: null,\r\n        content: null\r\n      },\r\n      settings: {\r\n        subscription: {\r\n          id: 'subscription',\r\n          title: 'Subscription (Example)',\r\n          data: [\r\n            {\r\n              text: 'Opened (your approval is not required)'\r\n            },\r\n            {\r\n              text: 'Opened (your approval is not required)'\r\n            },\r\n            {\r\n              text: 'Invite Only (your invitation required)'\r\n            }\r\n          ]\r\n        },\r\n        content: {\r\n          id: 'content',\r\n          title: 'Content',\r\n          data: [\r\n            {\r\n              text: 'Everybody can see your posts'\r\n            },\r\n            {\r\n              text: 'Only subscribers can see your posts'\r\n            },\r\n            {\r\n              text: 'Only members of privacy groups can see your posts'\r\n            },\r\n            {\r\n              text: 'Only you can see your posts'\r\n            }\r\n          ]\r\n        },\r\n        comments: {\r\n          id: 'Comments',\r\n          title: 'Comments',\r\n          data: [\r\n            {\r\n              text: 'Everybody can leave comments'\r\n            },\r\n            {\r\n              text: 'Only subscribers can leave comments'\r\n            },\r\n            {\r\n              text: 'Only members of privacy groups can leave comments'\r\n            },\r\n            {\r\n              text: 'Only you can leave comments'\r\n            }\r\n          ]\r\n        },\r\n        likes: {\r\n          id: 'Likes',\r\n          title: 'Likes',\r\n          data: [\r\n            {\r\n              text: 'Everybody can see posts you liked'\r\n            },\r\n            {\r\n              text: 'Only subscribers can see posts you liked'\r\n            },\r\n            {\r\n              text:\r\n                'Only members of privacy groups can see the posts you liked',\r\n              selectOptions: [\r\n                { id: 1, text: 'security group name' },\r\n                { id: 2, text: 'security group name' },\r\n                { id: 3, text: 'security group name' },\r\n                { id: 4, text: 'security group name' },\r\n                { id: 5, text: 'security group name' }\r\n              ]\r\n            },\r\n            {\r\n              text: 'Only you can leave comments'\r\n            }\r\n          ]\r\n        },\r\n        users: {\r\n          id: 'users',\r\n          title: 'Users you follow',\r\n          data: [\r\n            {\r\n              text: 'Everybody can see users you follow'\r\n            },\r\n            {\r\n              text: 'Only subscribers can see users you follow'\r\n            },\r\n            {\r\n              text: 'Only members of privacy groups can see users you follow'\r\n            },\r\n            {\r\n              text: 'Only you can see users you follow'\r\n            }\r\n          ]\r\n        }\r\n      }\r\n    };\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.section-title {\r\n  font-size: var(--size-primary);\r\n  color: var(--text-primary);\r\n  font-family: 'OpenSans-Bold', sans-serif;\r\n  margin-bottom: var(--element-margin-l);\r\n  line-height: 2em;\r\n}\r\n.settings {\r\n  padding: var(--element-padding-xl);\r\n  &-container {\r\n    background-color: var(--BG-blog-post);\r\n    border-radius: var(--border-radius);\r\n    padding: var(--element-padding-xl);\r\n\r\n    &__btns {\r\n      padding-top: var(--element-padding-l);\r\n      position: relative;\r\n\r\n      button {\r\n        height: 30px;\r\n        min-width: 90px;\r\n      }\r\n\r\n      &:before {\r\n        content: '';\r\n        border-top: var(--border-width-thin) solid var(--divider-in-popup);\r\n        width: 430px;\r\n        position: absolute;\r\n        top: 0px;\r\n        left: 0px;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.radio-button-group {\r\n  margin-bottom: var(--element-margin-l);\r\n  font-size: var(--size-regular);\r\n  h4 {\r\n    font-family: 'OpenSans-Bold', sans-serif;\r\n    color: var(--text-primary);\r\n  }\r\n}\r\n\r\n::v-deep .select {\r\n  margin-left: var(--element-margin-xl);\r\n}\r\n\r\n.transparent {\r\n  margin-right: var(--element-margin-s);\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Blog"}]}