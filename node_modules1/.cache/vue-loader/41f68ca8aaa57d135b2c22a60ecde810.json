scribe')\"\r\n      v-else-if=\"isSubscribe && !isEditable\"\r\n    />\r\n    <form-button\r\n      type=\"submit\"\r\n      text=\"Edit\"\r\n      @click.native.stop=\"$emit('edit')\"\r\n      v-else\r\n    />\r\n    <p class=\"sidebar-info__desc\" v-html=\"description\">{{ description }}</p>\r\n    <h4 class=\"sidebar-info__subtitle\">My Links</h4>\r\n    <ul class=\"sidebar-info-list\">\r\n      <li\r\n        class=\"sidebar-info-list__item text-overflow\"\r\n        :key=\"index\"\r\n        v-for=\"(link, index) in links\"\r\n      >\r\n        <a\r\n          :href=\"link\"\r\n          :title=\"link\"\r\n          target=\"_blank\"\r\n          rel=\"noopener\"\r\n          class=\"sidebar-info__link\"\r\n          >{{ link }}</a\r\n        >\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n<script>\r\nimport FormButton from '~common/Buttons/FormButton';\r\nexport default {\r\n  components: {\r\n    FormButton\r\n  },\r\n  props: {\r\n    name: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    description: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    links: {\r\n      type: Array\r\n    },\r\n    isSubscribe: {\r\n      type: Boolean\r\n    },\r\n    isEditable: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.sidebar-info {\r\n  background-color: var(--BG-blog-post);\r\n  display: flex;\r\n  flex-direction: column;\r\n  border-radius: var(--border-radius);\r\n  padding: var(--element-padding-l);\r\n  color: var(--text-primary);\r\n  font-size: var(--size-regular);\r\n  &__title {\r\n    font-family: 'OpenSans-Bold';\r\n    font-size: var(--size-primary);\r\n    text-align: center;\r\n    margin-bottom: var(--element-margin-m);\r\n  }\r\n  &__desc {\r\n    line-height: 1.5em;\r\n    margin: var(--element-margin-m) 0;\r\n  }\r\n  &__subtitle {\r\n    font-family: 'OpenSans-Bold';\r\n    margin-bottom: var(--element-margin-s);\r\n  }\r\n  &-list {\r\n    list-style: none;\r\n    margin: 0;\r\n    &__item {\r\n      margin-bottom: var(--element-margin-s);\r\n    }\r\n  }\r\n  &__link {\r\n    color: var(--text-secondary);\r\n  }\r\n}\r\n</style>\r\n<i18n>\r\n{\r\n  \"en\": {\r\n    \"Subscribe\": \"Subscribe\",\r\n    \"Unsubscribe\": \"Unsubscribe\",\r\n    \"myLinks\": \"My Links\"\r\n  },\r\n  \"ru\": {\r\n    \"Subscribe\": \"Подписаться\",\r\n    \"Unsubscribe\": \"Отменить подписку\",\r\n    \"myLinks\": \"Мои ссылки\"\r\n  }\r\n}\r\n</i18n>\r\n"]}]}