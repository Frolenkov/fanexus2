<div class=\"modal\" v-click-outside=\"() => close({ clickOutside: true })\">\r\n        <slot name=\"modal-image\" />\r\n        <div class=\"modal-content\">\r\n          <header class=\"modal-header\">\r\n            <slot name=\"header\" v-if=\"!settings.hideHeader\">\r\n              <h2 class=\"modal-header__title\" v-if=\"settings.title\">\r\n                {{ settings.title }}\r\n              </h2>\r\n            </slot>\r\n            <button\r\n              type=\"button\"\r\n              class=\"modal-header-btn__close\"\r\n              @click=\"close\"\r\n            ></button>\r\n          </header>\r\n          <section class=\"modal-body\">\r\n            <slot name=\"body\"></slot>\r\n          </section>\r\n          <footer class=\"modal-footer\">\r\n            <slot name=\"footer\">\r\n              <button type=\"button\" v-if=\"settings.dismissText\" @click=\"close\">\r\n                {{ settings.dismissText }}\r\n              </button>\r\n              <button type=\"submit\" v-if=\"settings.actionText\" @click=\"save\">\r\n                {{ settings.actionText }}\r\n              </button>\r\n            </slot>\r\n          </footer>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n<script>\r\nimport { APP_TITLE } from '@/services/config';\r\nimport ClickOutside from 'vue-click-outside';\r\n\r\nexport default {\r\n  props: {\r\n    settings: {\r\n      type: Object,\r\n      default() {\r\n        return {\r\n          title: APP_TITLE,\r\n          hideHeader: false,\r\n          show: false,\r\n          auth: false\r\n        };\r\n      },\r\n      required: true\r\n    },\r\n    isCloseOutside: {\r\n      type: Boolean,\r\n      default: true\r\n    }\r\n  },\r\n  directives: {\r\n    ClickOutside\r\n  },\r\n  methods: {\r\n    close({ clickOutside = false }) {\r\n      if (clickOutside && !this.isCloseOutside) return;\r\n\r\n      this.settings.show = false;\r\n      this.$emit('close');\r\n    },\r\n    save() {\r\n      /*@TODO add save method */\r\n      this.$emit('save');\r\n      this.close();\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.modal {\r\n  overflow-x: auto;\r\n  display: flex;\r\n\r\n  &-overlay {\r\n    position: fixed;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    top: 0;\r\n    left: 0;\r\n    background-color: var(--popup-overlay);\r\n    overflow: auto;\r\n    z-index: var(--z-index-popup);\r\n\r\n    &.auth .modal {\r\n      background: var(--color-white);\r\n    }\r\n\r\n    &.auth {\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n\r\n      .modal-header {\r\n        position: relative;\r\n        height: 20px;\r\n      }\r\n    }\r\n    /*@TODO revise styles if needed since blog-post is a separate component*/\r\n    &.blog-post {\r\n      .modal {\r\n        width: 780px;\r\n        overflow: visible;\r\n        position: relative;\r\n        margin: 20px auto;\r\n        &-content {\r\n          margin: 0;\r\n        }\r\n        &-header {\r\n          margin: 0;\r\n          &-btn__close {\r\n            top: 20px;\r\n            right: 20px;\r\n            transform: initial;\r\n            z-index: var(--z-index-1);\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n  &-content {\r\n    margin: 0 var(--element-margin-xxl);\r\n  }\r\n  &-body {\r\n    position: relative;\r\n  }\r\n  &-header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin: var(--element-margin-xl) 0;\r\n    &-btn {\r\n      &__close {\r\n        background: unset;\r\n        cursor: pointer;\r\n        width: 20px;\r\n        height: 20px;\r\n        align-self: flex-start;\r\n        position: absolute;\r\n        top: 0;\r\n        right: 0;\r\n        transform: translateX(100%);\r\n\r\n        &:before,\r\n        &:after {\r\n          position: absolute;\r\n          content: '';\r\n          top: 50%;\r\n          left: 50%;\r\n          width: 2px;\r\n          height: 20px;\r\n          background: var(--text-primary);\r\n          border-radius: 2px;\r\n        }\r\n        &:before {\r\n          transform: translate(-50%, -50%) rotate(45deg);\r\n        }\r\n        &:after {\r\n          transform: translate(-50%, -50%) rotate(-45deg);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}