A;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA","file":"AsideMenu.vue","sourceRoot":"src/components/Layout","sourcesContent":["<template>\r\n  <aside class=\"aside-menu\" :class=\"{ toggled: show }\">\r\n    <div class=\"aside-menu__fixed\">\r\n      <button type=\"button\" class=\"menu-toggler\" @click=\"handleAsideToggled\">\r\n        <span></span>\r\n      </button>\r\n      <div v-if=\"userAliases\" class=\"icons-bar\">\r\n        <div\r\n          :key=\"id\"\r\n          v-for=\"{ id, avatar: { src } } in userAliases\"\r\n          class=\"icons-bar__image\"\r\n          :class=\"{ active: activeAlias(id) }\"\r\n          @click=\"aliasClickHandler(id)\"\r\n        >\r\n          <!-- /*@TODO add dynamic data*/ -->\r\n          <avatar size=\"md\" :file=\"src\" />\r\n          <!-- <badge :info=\"12\" /> @TODO resolve after FAN-175 will be resolved -->\r\n        </div>\r\n        <button\r\n          @click.stop=\"showAddAliasPopup\"\r\n          class=\"icons-bar__image icons-bar__image--new-account\"\r\n        >\r\n          <plus-icon />\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <div class=\"aside-menu__toggler\">\r\n      <nav class=\"aside-menu-navigation\">\r\n        <h3>{{ $t('joker') }}</h3>\r\n        <ul>\r\n          <li v-if=\"myBlog\" class=\"aside-menu-navigation__link\">\r\n            <router-link\r\n              class=\"header-navigation__link\"\r\n              :to=\"{ name: 'blog', params: { name: myBlog.id } }\"\r\n              >{{ $t('myblog') }}<badge :info=\"34\" />\r\n            </router-link>\r\n          </li>\r\n          <li class=\"aside-menu-navigation__link\">\r\n            <router-link :to=\"{ name: 'MyCommunities' }\">{{\r\n              $t('mycommun')\r\n            }}</router-link>\r\n          </li>\r\n          <li class=\"aside-menu-navigation__link\">\r\n            <router-link :to=\"{ name: 'Following' }\">{{\r\n              $t('following')\r\n            }}</router-link>\r\n          </li>\r\n          <li class=\"aside-menu-navigation__link\">\r\n            <router-link :to=\"{ name: 'Followers' }\"\r\n              >{{ $t('followers') }}\r\n              <!-- /*@TODO add dynamic data*/ -->\r\n              <badge :info=\"1555\"\r\n            /></router-link>\r\n          </li>\r\n          <li class=\"aside-menu-navigation__link\">\r\n            <a href=\"#\">{{ $t('activities') }}</a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n      <div class=\"aside-menu__btns\">\r\n        <div class=\"aside-menu__theme-toggler\">\r\n          Dark mode<form-toggler\r\n            name=\"theme\"\r\n            class=\"form-toggler\"\r\n            @change=\"toggleTheme\"\r\n            :checked=\"themeDark\"\r\n          />\r\n        </div>\r\n        <button\r\n          type=\"button\"\r\n          class=\"aside-menu__logout-btn\"\r\n          @click=\"logoutBtnHandler\"\r\n        >\r\n          Logout\r\n          <logout-icon class=\"logout-icon\" />\r\n        </button>\r\n        <a href=\"#\" @click=\"setLocale('en')\">EN</a>\r\n        <a href=\"#\" @click=\"setLocale('ru')\">RU</a>\r\n      </div>\r\n    </div>\r\n    <alias-modal\r\n      :settings=\"aliasModalSettings\"\r\n      v-if=\"aliasModalSettings.show\"\r\n      :data=\"aliasPopupModalData\"\r\n      @sendNewAliasData=\"sendNewAliasData\"\r\n    />\r\n  </aside>\r\n</template>\r\n<script>\r\nimport { mapState, mapActions } from 'vuex';\r\nimport { getMyBlog } from '@/services/alias.service';\r\nimport Badge from '~common/Badge';\r\nimport Avatar from '~common/Avatar';\r\nimport AliasModal from '~common/AliasModal';\r\nimport PlusIcon from '~svg/plus.svg';\r\nimport LogoutIcon from '~svg/logout.svg';\r\nimport FormToggler from '~common/Fields/FormToggler';\r\nimport { createNewAlias } from '@/services/alias.service';\r\n\r\nexport default {\r\n  name: 'fanexus-aside-menu',\r\n  components: {\r\n    PlusIcon,\r\n    FormToggler,\r\n    LogoutIcon,\r\n    Avatar,\r\n    Badge,\r\n    AliasModal\r\n  },\r\n  data() {\r\n    return {\r\n      myBlog: null,\r\n      aliasModalSettings: {\r\n        show: false,\r\n        title: 'Alias',\r\n        dismissText: 'Cancel',\r\n        actionText: 'Save'\r\n      },\r\n      aliasPopupModalData: {\r\n        text:\r\n          'Alias`s image may be changed (it will also be placed at the blog). Alias name can`t be changed (you may change it in the blog).',\r\n        imageInputValue: '',\r\n        aliasName: ''\r\n      }\r\n    };\r\n  },\r\n  props: {\r\n    show: {\r\n      type: Boolean,\r\n      default: false,\r\n      required: true\r\n    }\r\n  },\r\n  methods: {\r\n    /*@TODO add a corresponding side effect to the login action (if needed) */\r\n    ...mapActions([\r\n      'logout',\r\n      'toggleTheme',\r\n      'getUserAliases',\r\n      'setActiveAlias',\r\n      'initActiveAlias',\r\n      'removeActiveAlias',\r\n      'clearUserAliases'\r\n    ]),\r\n    setLocale(locale) {\r\n      this.$i18n.locale = locale;\r\n    },\r\n    handleAsideToggled() {\r\n      this.$emit('asideToggled');\r\n    },\r\n    showAddAliasPopup() {\r\n      this.aliasModalSettings.show = true;\r\n    },\r\n    aliasClickHandler(newActiveAliasId) {\r\n      if (newActiveAliasId !== this.activeAliasId) {\r\n        this.setActiveAlias(newActiveAliasId);\r\n      }\r\n    },\r\n    async logoutBtnHandler() {\r\n      await this.logout();\r\n      this.removeActiveAlias();\r\n      this.clearUserAliases();\r\n    },\r\n    async sendNewAliasData() {\r\n      // @TODO change avatar.src after BE will be able to give us an image link after upload\r\n      const newAliasData = {\r\n        name: this.aliasPopupModalData.aliasName,\r\n        avatar: {\r\n          src:\r\n            'https://upload.wikimedia.org/wikipedia/en/thumb/7/7b/Batman_%28DC_Rebirth_version%29.png/220px-Batman_%28DC_Rebirth_version%29.png'\r\n        }\r\n      };\r\n      try {\r\n        await createNewAlias(newAliasData);\r\n        this.aliasModalSettings.show = false;\r\n        await this.getUserAliases();\r\n      } catch (e) {\r\n        return false;\r\n      }\r\n    },\r\n    async updateMyBlogLink(alias) {\r\n      this.myBlog = await getMyBlog({ alias });\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      themeDark: state => state.theme.themeDark,\r\n      userAliases: state => state.alias.userAliases,\r\n      activeAliasId: state => state.alias.activeAliasId,\r\n      isAuthenticated: state => state.auth.isAuthenticated\r\n    }),\r\n    activeAlias() {\r\n      return id => this.activeAliasId === id;\r\n    }\r\n  },\r\n  watch: {\r\n    activeAliasId: {\r\n      handler(val) {\r\n        this.updateMyBlogLink(val);\r\n      },\r\n      immediate: true\r\n    }\r\n  },\r\n  async mounted() {\r\n    if (this.isAuthenticated) {\r\n      await this.getUserAliases();\r\n      if (!this.activeAliasId) {\r\n        this.initActiveAlias();\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.wrapper.logged .aside-menu {\r\n  display: flex;\r\n}\r\n.aside-menu {\r\n  grid-area: aside;\r\n  display: none;\r\n  overflow: hidden;\r\n  width: 60px;\r\n  transition: var(--transition-default);\r\n\r\n  &.toggled {\r\n    width: 260px;\r\n    .aside-menu__toggler {\r\n      left: 60px;\r\n    }\r\n  }\r\n\r\n  &.toggled .menu-toggler:before {\r\n    transform: rotate(45deg);\r\n    left: 0px;\r\n    top: 6px;\r\n  }\r\n\r\n  &.toggled .menu-toggler:after {\r\n    opacity: 0;\r\n  }\r\n\r\n  &.toggled .menu-toggler span {\r\n    transform: rotate(-45deg);\r\n    left: 0px;\r\n    top: 6px;\r\n  }\r\n\r\n  &__fixed {\r\n    position: relative;\r\n    z-index: var(--z-index-1);\r\n    width: 60px;\r\n    background: var(--BG-navbar);\r\n\r\n    .menu-toggler {\r\n      display: block;\r\n      width: 19px;\r\n      height: 12px;\r\n      position: relative;\r\n      margin: var(--element-margin-l) auto;\r\n      background: none;\r\n      transition: var(--transition-default);\r\n      &:focus {\r\n        outline: none;\r\n      }\r\n      &:focus {\r\n        span {\r\n          background-color: var(--BG-button-primary-hover);\r\n        }\r\n        &:before,\r\n        &:after {\r\n          background-color: var(--BG-button-primary-hover);\r\n        }\r\n      }\r\n\r\n      &:before,\r\n      &:after,\r\n      & span {\r\n        position: absolute;\r\n        height: 2px;\r\n        width: 19px;\r\n        top: 0;\r\n        left: 0;\r\n        content: '';\r\n        border-radius: var(--border-radius);\r\n        background-color: var(--color-white);\r\n        transition: var(--transition-default);\r\n      }\r\n\r\n      &:after {\r\n        top: 5px;\r\n      }\r\n\r\n      & span {\r\n        top: 10px;\r\n      }\r\n    }\r\n\r\n    .icons-bar {\r\n      padding: var(--element-padding-s);\r\n      padding-top: var(--element-padding-l);\r\n\r\n      &__image {\r\n        width: 40px;\r\n        height: 40px;\r\n        border-radius: var(--border-radius);\r\n        margin-bottom: var(--element-margin-l);\r\n        position: relative;\r\n        cursor: pointer;\r\n        ::v-deep img {\r\n          border: var(--border-width-thin) solid transparent;\r\n        }\r\n\r\n        &--new-account {\r\n          background-color: var(--BG-button-aside);\r\n          display: flex;\r\n          justify-content: center;\r\n          align-items: center;\r\n          &:focus {\r\n            outline: none;\r\n          }\r\n        }\r\n\r\n        &.active {\r\n          ::v-deep img {\r\n            border: var(--border-width-thin) solid var(--color-white);\r\n          }\r\n        }\r\n\r\n        ::v-deep img {\r\n          height: 100%;\r\n          width: 100%;\r\n          object-position: top;\r\n        }\r\n\r\n        ::v-deep .badge {\r\n          position: absolute;\r\n          top: -10px;\r\n          right: 0;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  &__toggler {\r\n    padding-bottom: var(--element-padding-xxl);\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    position: absolute;\r\n    height: 100%;\r\n    left: -140px;\r\n    width: var(--width-small);\r\n    padding-top: 60px;\r\n    padding-left: var(--element-padding-s);\r\n    background-color: var(--BG-aside-togglable);\r\n    transition: var(--transition-default);\r\n    box-shadow: var(--box-shadow);\r\n  }\r\n\r\n  &__btns {\r\n    padding-left: var(--element-padding-s);\r\n    font-size: var(--size-regular);\r\n  }\r\n\r\n  &__theme-toggler {\r\n    margin-bottom: var(--element-margin-m);\r\n    margin-top: var(--element-margin-m);\r\n    color: var(--color-white);\r\n\r\n    .form-toggler {\r\n      vertical-align: middle;\r\n      margin-left: var(--element-margin-s);\r\n    }\r\n  }\r\n\r\n  &__logout-btn {\r\n    background: none;\r\n    color: var(--color-white);\r\n    margin-bottom: var(--element-margin-m);\r\n    margin-top: var(--element-margin-m);\r\n    &:focus {\r\n      outline: none;\r\n      color: var(--color-main-500);\r\n      .logout-icon {\r\n        fill: var(--color-main-500);\r\n      }\r\n    }\r\n\r\n    .logout-icon {\r\n      margin-left: var(--element-margin-xs);\r\n      vertical-align: bottom;\r\n      height: 12px;\r\n      fill: var(--color-white);\r\n    }\r\n  }\r\n\r\n  h3 {\r\n    padding: var(--element-padding-s);\r\n    background-color: rgba(255, 255, 255, 0.25);\r\n    border-top-left-radius: var(--border-radius);\r\n    border-bottom-left-radius: var(--border-radius);\r\n    margin-bottom: var(--element-margin-xl);\r\n    color: var(--color-white);\r\n    font-size: var(--size-regular);\r\n    font-family: 'OpenSans-Bold', sans-serif;\r\n    line-height: 21px;\r\n  }\r\n\r\n  &-navigation {\r\n    padding-top: var(--element-padding-s);\r\n    list-style: none;\r\n\r\n    &__link {\r\n      margin-bottom: var(--element-padding-s);\r\n      display: flex;\r\n      align-items: center;\r\n\r\n      ::v-deep .badge {\r\n        padding: 0 calc(var(--button-padding-m) / 1.5);\r\n        margin-left: 15px;\r\n        display: block;\r\n      }\r\n\r\n      ::v-deep a {\r\n        padding: var(--element-padding-s);\r\n        border-top-left-radius: var(--border-radius);\r\n        border-bottom-left-radius: var(--border-radius);\r\n        text-decoration: none;\r\n        color: var(--color-white);\r\n        font-size: var(--size-regular);\r\n        line-height: 21px;\r\n        width: 100%;\r\n        display: flex;\r\n        transition: var(--transition-default);\r\n\r\n        &.router-link-exact-active {\r\n          background-color: var(--BG-navbar);\r\n          border-top-left-radius: var(--border-radius);\r\n          border-bottom-left-radius: var(--border-radius);\r\n        }\r\n\r\n        &:hover {\r\n          background-color: var(--BG-navbar);\r\n        }\r\n\r\n        &:focus {\r\n          background-color: var(--BG-navbar);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}