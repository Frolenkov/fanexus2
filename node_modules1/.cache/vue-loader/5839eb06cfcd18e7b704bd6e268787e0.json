editTagsModalSettings.show\"\r\n      :settings=\"editTagsModalSettings\"\r\n    />\r\n    <blog-post-modal\r\n      v-if=\"blogPostData.id && blogPostEditMode === modes['show']\"\r\n      :id=\"blogPostData.id\"\r\n      :settings=\"blogModalSettings\"\r\n    />\r\n    <auth\r\n      v-if=\"authModalSettings.show\"\r\n      :settings=\"authModalSettings\"\r\n      @submit=\"handleAuthSubmit\"\r\n    />\r\n    <slot />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { APP_TITLE } from '@/services/config';\r\nimport Auth from '@/components/Auth/Auth';\r\nimport FanexusHeader from '@/components/Layout/Header';\r\nimport FanexusAsideMenu from '@/components/Layout/AsideMenu';\r\nimport BlogPostModal from '@/components/Blog/BlogPostModal.vue';\r\nimport { mapActions, mapState } from 'vuex';\r\nimport CreatePostModal from '../components/Activities/CreateBlogPostModal';\r\nimport EditTagsModal from '../components/Activities/EditTagsModal';\r\nimport { modes } from '@/services/BlogPost/blogPost.service';\r\n\r\nexport default {\r\n  components: {\r\n    Auth,\r\n    CreatePostModal,\r\n    BlogPostModal,\r\n    FanexusHeader,\r\n    FanexusAsideMenu,\r\n    EditTagsModal\r\n  },\r\n  data() {\r\n    const context = this;\r\n    return {\r\n      asideToggled: false,\r\n      authModalSettings: {\r\n        title: APP_TITLE,\r\n        show: false,\r\n        auth: true,\r\n        initialTab: []\r\n      },\r\n      blogModalSettings: {\r\n        hideHeader: true\r\n      },\r\n      createPostModalSettings: {\r\n        get show() {\r\n          return ['edit', 'create']\r\n            .map(mode => modes[mode])\r\n            .includes(context.blogPostEditMode);\r\n        },\r\n        set show(value) {\r\n          if (!value) {\r\n            if (context.$route.params.postId) {\r\n              context.setMode('show');\r\n            } else {\r\n              context.setMode(null);\r\n            }\r\n          }\r\n        },\r\n        get title() {\r\n          return `${context.blogPostEditMode} post`;\r\n        },\r\n        dismissText: 'Cancel',\r\n        actionText: 'Publish'\r\n      },\r\n      editTagsModalSettings: {\r\n        title: 'Edit tags',\r\n        dismissText: 'Cancel',\r\n        actionText: 'Save tags',\r\n        get show() {\r\n          return context.showEditTagsModal;\r\n        },\r\n        set show(value) {\r\n          context.openEditTagsModal(value);\r\n        }\r\n      },\r\n      modes: modes\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      isAuthenticated: state => state.auth.isAuthenticated,\r\n      showEditTagsModal: state => state.tags.showEditTagsModal,\r\n      blogPostId: state => state.blogPost.data.id,\r\n      blogPostEditContent: state => state.blogPost.data.content,\r\n      blogPostData: state => state.blogPost.data,\r\n      blogPostEditMode: state => state.blogPost.mode\r\n    })\r\n  },\r\n  methods: {\r\n    ...mapActions([\r\n      'openEditTagsModal',\r\n      'setCreateBlogPost',\r\n      'setBlogPostId',\r\n      'setMode'\r\n    ]),\r\n    handleOpenAuthModal({ tabIndex, componentIndex }) {\r\n      this.authModalSettings.initialTab = [tabIndex, componentIndex];\r\n      this.authModalSettings.show = true;\r\n    },\r\n    /*@TODO revise to respect the create account option*/\r\n    handleAuthSubmit() {\r\n      this.authModalSettings.show = false;\r\n    },\r\n    handleAsideToggled() {\r\n      this.asideToggled = !this.asideToggled;\r\n    },\r\n    handleOpenCreatePostModal() {\r\n      this.setCreateBlogPost();\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}