activeTab\"\r\n      />\r\n    </div>\r\n    <div class=\"edit-community__content\">\r\n      <div class=\"edit-community__content-wrapper\">\r\n        <sidebar-form\r\n          :show-buttons=\"true\"\r\n          :errors=\"errors\"\r\n          :form-data=\"data\"\r\n          @save=\"onSave\"\r\n        />\r\n      </div>\r\n      <div class=\"edit-community__content-main\">\r\n        <!-- @TODO revise usage of formData vs data here  */ -->\r\n        <component\r\n          :is=\"currentComponent\"\r\n          :showButtons=\"false\"\r\n          :formData=\"data\"\r\n          :data=\"data\"\r\n          :settings=\"settings\"\r\n        />\r\n      </div>\r\n      <div class=\"edit-community__content-wrapper\">\r\n        <sidebar-tags\r\n          class=\"edit-community__content-tags\"\r\n          createText=\"Tags of your posts will be shown here. You may add them, but others will see them only when you use them in your posts.\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport settings from '@/services/Community/community.settings';\r\nimport AddImage from '../common/AddImage';\r\nimport Tabs from '../common/Tabs';\r\nimport tabsMixin from '@/mixins/tabs.mixin';\r\nimport SidebarTags from '../common/SidebarTags';\r\nimport SidebarForm from '../common/SidebarForm';\r\nimport CommunityCreateRules from '@/components/Community/CommunityCreateRules';\r\nimport CommunitySettings from '@/components/Community/CommunitySettings';\r\nimport {\r\n  getCommunity,\r\n  communityAreas,\r\n  editCommunity,\r\n  communityRoot\r\n} from '@/services/community.service';\r\nimport getComponentsByPrefix from '@/utils/getComponentsByPrefix';\r\nexport default {\r\n  name: 'CommunityEditCommunity',\r\n  components: {\r\n    SidebarForm,\r\n    AddImage,\r\n    Tabs,\r\n    CommunityCreateRules,\r\n    CommunitySettings,\r\n    SidebarTags\r\n  },\r\n  props: {\r\n    id: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  mixins: [tabsMixin],\r\n  data() {\r\n    return {\r\n      errors: [],\r\n      data: null,\r\n      activeTab: 0,\r\n      currentComponent: null,\r\n      /** @TODO revise format usage. check @see 'src\\services\\Community\\community.rules.js'  */\r\n      rules: [\r\n        {\r\n          ruleName: '',\r\n          ruleDescription: ''\r\n        }\r\n      ],\r\n      settings,\r\n      navTabs: [\r\n        {\r\n          title: 'The rules',\r\n          component: communityAreas.createRules\r\n        },\r\n        {\r\n          title: 'Settings',\r\n          component: communityAreas.settings\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    communityComponents() {\r\n      return getComponentsByPrefix.call(this, communityRoot);\r\n    }\r\n  },\r\n  methods: {\r\n    setComponent(tabIndex = 0, componentIndex = 0) {\r\n      this.handleSetActiveTab(tabIndex);\r\n      this.currentComponent = this.communityComponents[componentIndex];\r\n    },\r\n    async getData() {\r\n      this.data = await getCommunity(this.id, 1);\r\n      this.data.links = this.data.links.map(item => ({ link: item }));\r\n      this.data.rules = this.data.rules.map(item => ({\r\n        /** @TODO revise. check the console error @see http://prntscr.com/spw9nu */\r\n        ruleName: item.split('<h3>')[1].split('</h3>')[0],\r\n        ruleDescription: item.split('<br>')[1]\r\n      }));\r\n      this.data.settings = this.settings;\r\n    },\r\n    async onSave() {\r\n      /** @TODO revise */\r\n      const rules = this.data.rules.map(\r\n        rule => `<p><h2>${rule.ruleName}</h2><br>${rule.ruleDescription}</p>`\r\n      );\r\n      const payload = {\r\n        rules: rules,\r\n        ...this.data\r\n      };\r\n      this.data = await editCommunity(this.id, payload);\r\n      if (this.data.errors) {\r\n        this.errors = this.data.errors;\r\n      } else {\r\n        await this.$router.push(`/community/${this.id}`);\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.setComponent();\r\n    this.getData();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.edit-community {\r\n  grid-area: content;\r\n  overflow: auto;\r\n  background-color: var(--BG-primary);\r\n  &__add-bg {\r\n    ::v-deep .button {\r\n      padding: 8px var(--element-padding-xxl);\r\n    }\r\n  }\r\n  &__header {\r\n    display: grid;\r\n    align-items: end;\r\n    grid-template-rows: minmax(0, 60px);\r\n    grid-template-columns: var(--width-small) max-content var(--width-small);\r\n    grid-gap: var(--element-margin-xl);\r\n    background-color: var(--BG-blog-post);\r\n    padding: 0 var(--element-padding-xl);\r\n    margin-bottom: var(--element-margin-xl);\r\n  }\r\n\r\n  &__content {\r\n    padding: 0 var(--element-padding-xl);\r\n    display: grid;\r\n    grid-gap: var(--element-padding-xl);\r\n    grid-template-columns: var(--width-small) auto var(--width-small);\r\n  }\r\n}\r\n</style>\r\n"]}]}